.comp-navbar {
  position: fixed;
  right: 0;
  left: 0;
  top: 0;
  z-index: 9999;
}
.comp-navbar .nav-statusbar {
  background: #fff;
  position: relative;
}
.comp-navbar .nav-statusbar.transparent {
  background: transparent;
}
.comp-navbar .nav-titlebar {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  background: #fff;
}
.comp-navbar .nav-titlebar.transparent {
  background: transparent;
}
.comp-navbar .nav-titlebar .bar-title {
  width: 240rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}
.comp-navbar .nav-titlebar .bar-title.tit-center {
  height: 44rpx;
  line-height: 44rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.comp-navbar .nav-titlebar .bar-options {
  min-width: 230rpx;
  height: 60rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: absolute;
  left: 12px;
  background: transparent;
  border-radius: 27px;
  padding-right: 5rpx;
}
.comp-navbar .nav-titlebar .bar-options.android {
  background: hsla(0, 0%, 100%, 0);
}
.comp-navbar .nav-titlebar .bar-options .opt {
  width: 48rpx;
  height: 68rpx;
  overflow: visible;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-home {
  width: 56rpx;
  height: 56rpx;
  margin-right: 20rpx;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-back {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 20rpx;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-back .back-image {
  width: 18rpx;
  height: 34rpx;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-back .back-image.back-close {
  width: 48rpx;
  height: 48rpx;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-search {
  width: 270rpx;
  height: 56rpx;
  border-radius: 28rpx;
  margin-left: 6rpx;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-search .cover-input {
  width: 100%;
  height: 56rpx;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-search .cover-input .text {
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  padding-left: 63rpx;
  background: #eeeff0;
  border-radius: 28rpx;
  box-sizing: border-box;
  color: #aeaeae;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-search .searchImg {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  left: 16rpx;
  top: 50%;
  transform: translateY(-50%);
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention {
  width: auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .userbox {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .userbox .imgbox {
  position: relative;
  width: 54rpx;
  height: 68rpx;
  display: flex;
  align-items: center;
  overflow: visible;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .userbox .imgbox .attention-img {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .userbox .imgbox .icon {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 0rpx;
  bottom: 8rpx;
  z-index: 9999;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .userbox .user {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.85);
  line-height: 45rpx;
  margin-right: 25rpx;
  max-width: 280rpx;
  padding-left: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .operate {
  width: 85rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  border-radius: 18rpx;
  border: 1rpx solid #e9ae0a;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #e9ae0a;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .operate.follow {
  width: 96rpx;
  height: 36rpx;
  line-height: 36rpx;
  background: #e8e8e8;
  border: 1rpx solid #e8e8e8;
  border-radius: 18rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #a9a9a9;
}
.comp-navbar .nav-titlebar .bar-options .opt.opt-attention .operate.disabled {
  border: 1rpx solid #a9a9a9;
  color: #a9a9a9;
}
.placeholder-bar {
  background-color: transparent;
  width: 100%;
}
.text-overflow {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
