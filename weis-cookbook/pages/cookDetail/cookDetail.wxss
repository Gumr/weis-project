.mb72 {
  margin-bottom: 72rpx;
}
.mb50 {
  margin-bottom: 50rpx;
}
.flex {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.text-overflow {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
page {
  width: 100%;
  overflow-x: hidden;
}
.cookDetail {
  padding-bottom: 116rpx;
}
.container {
  left: 0;
  right: 0;
}
.container .cookImg {
  margin: 0 -30rpx 50rpx;
  position: relative;
}
.container .cookImg image {
  width: 750rpx;
  vertical-align: top;
}
.container .cookImg .cookStatus {
  width: 750rpx;
  height: 72rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx 0 0;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  left: 0;
  top: 0;
  z-index: 9999;
}
.container .cookImg .cookStatus.violation {
  background: rgba(250, 100, 0, 0.8);
  color: #fff;
  font-size: 24rpx;
}
.container .cookImg .cookStatus.noword {
  background: transparent;
}
.container .cookImg .cookStatus .text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 33rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .cookImg .cookStatus .text image {
  width: 180rpx;
  margin-right: 8rpx;
}
.container .cookImg .cookStatus .otherStatus {
  width: 138rpx;
  height: 46rpx;
  margin-left: -30rpx;
}
.container .cookImg .cookStatus .more {
  width: 60rpx;
  height: 60rpx;
}
.container .cookImg .databox {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-around;
  height: 160rpx;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  left: 0;
  bottom: 0;
}
.container .cookImg .databox .goodimg {
  width: 130rpx;
  height: 130rpx;
  border-radius: 20rpx;
  margin-right: 37rpx;
  flex-shrink: 0;
  margin: 0 36rpx 0 32rpx;
}
.container .cookImg .databox .datas {
  flex-grow: 1;
  max-width: 100%;
  overflow: hidden;
}
.container .cookImg .databox .datas .name {
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  color: #ffffff;
  line-height: 36rpx;
  margin-bottom: 10rpx;
}
.container .cookImg .databox .datas .info {
  max-width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .cookImg .databox .datas .info > text {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 0.7);
  line-height: 30rpx;
}
.container .cookImg .databox .datas .info > text .txt {
  font-size: 24rpx;
  color: #FCCC49;
  opacity: 1;
}
.container .cookImg .databox .datas .info.special > text {
  margin-right: 20rpx;
}
.container .cookImg .databox .arrow {
  width: 36rpx;
  height: 36rpx;
  margin-right: 28rpx;
  flex-shrink: 0;
}
.container .cookImg .databox.hasVideo {
  height: 200rpx;
  position: static;
  background: #FFFFFF;
  box-shadow: 0px 2px 24px -6px rgba(186, 186, 186, 0.3);
}
.container .cookImg .databox.hasVideo .datas .name {
  color: #000;
}
.container .cookImg .databox.hasVideo .datas .info > text {
  color: #000000;
}
.container .cookImg .databox.hasVideo .datas .info > text .txt {
  font-size: 24rpx;
  color: #FCCC49;
  opacity: 1;
}
.container .cookName .tip {
  margin-bottom: 24rpx;
}
.container .cookName .tip image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
  vertical-align: middle;
}
.container .cookName .tip .pre {
  color: #E9AE0A;
}
.container .cookName .tip text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 24rpx;
}
.container .cookName .box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 18rpx;
}
.container .cookName .box h4 {
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 40rpx;
  display: block;
}
.container .cookName .box .share {
  width: 130rpx;
  height: 48rpx;
  background: linear-gradient(270deg, #fee880 0%, #fccc49 100%);
  border-radius: 24rpx;
  margin: 0;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: center;
}
.container .cookName .box .share image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.container .cookName .box .share text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  vertical-align: middle;
}
.container .cookName .see {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 50rpx;
}
.container .cookName .see image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 13rpx;
}
.container .cookName .see text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 40rpx;
}
.container .cookName p {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 37rpx;
}
.container .materials .title {
  margin-bottom: 8rpx !important;
}
.container .materials .list .each {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.85);
  line-height: 64rpx;
}
.container .materials .list .each view {
  width: 50%;
}
.container .steps .step .imgbox {
  width: 690rpx;
  height: 400rpx;
  position: relative;
  margin-bottom: 40rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.container .steps .step .imgbox .stepImg {
  width: 100%;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.container .steps .step .desc {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 37rpx;
}
.container .steps .step .label text {
  font-size: 32rpx;
  color: #A9A9A9;
}
.container .steps .step .label text:first-child {
  color: #000 !important;
}
.container .tips {
  margin-bottom: 60rpx;
}
.container .tips .content {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 37rpx;
}
.container .createTime {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 37rpx;
  margin-bottom: 63rpx;
}
.container .createTime.showComments {
  margin: 0;
  padding: 0;
}
.container .operate {
  width: 100%;
  padding: 15rpx 30rpx 0;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  background: #fff;
}
.container .operate .wrap {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-around;
}
.container .operate .wrap .each {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-left: -32rpx;
}
.container .operate .wrap .each image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 12rpx;
}
.container .operate .wrap .each text {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 33rpx;
}
.container .operate .wrap .each .comment {
  width: 280rpx;
  height: 64rpx;
  line-height: 64rpx;
  background: #efefef;
  border-radius: 32rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #6d7278;
  padding-left: 32rpx;
  box-sizing: border-box;
  margin-right: 40rpx;
}
.container .operate .wrap .each .icons {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .operate .wrap .each.cantvote text {
  color: #666666;
}
.title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 32rpx;
}
.title image {
  width: 20rpx;
  height: 32rpx;
  margin-right: 8rpx;
}
.title .label {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 40rpx;
}
.publish-kb {
  width: 750rpx;
  padding: 20rpx 37rpx 20rpx 20rpx;
  box-sizing: border-box;
  background: #ffffff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  opacity: 0;
  transition: all 0.3s;
}
.publish-kb.show {
  opacity: 1;
}
.publish-kb textarea {
  width: 606rpx;
  min-height: 70rpx;
  line-height: 30rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  background: #efefef;
  border-radius: 16rpx;
  padding: 20rpx 0 20rpx 22rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
}
.publish-kb .submit {
  font-size: 28rpx;
  font-weight: 400;
  color: #FA6400;
  line-height: 40rpx;
}
.publish-kb .submit.disabled {
  color: #999999;
}
.pop-ups {
  background: #fff;
  border-radius: 20rpx 20rpx 0px 0px;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 1000;
}
.pop-ups .options {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-between;
  padding: 0 152rpx;
  box-sizing: border-box;
  margin: 90rpx 0 112rpx;
}
.pop-ups .options .opt {
  text-align: center;
}
.pop-ups .options .opt image {
  width: 110rpx;
  height: 110rpx;
  margin-bottom: 8rpx;
}
.pop-ups .options .opt .txt {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 36rpx;
}
.pop-ups .cancel {
  width: 420rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #e0e0e0;
  border-radius: 44rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #a9a9a9;
}
.delete {
  width: 600rpx;
  height: 390rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 0 auto;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1000;
}
.delete .tips {
  margin: 114rpx 0;
  text-align: center;
}
.delete .options {
  padding: 0 36rpx;
  box-sizing: border-box;
  margin-bottom: 48rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-between;
}
.delete .options .opt {
  width: 254rpx;
  height: 72rpx;
  line-height: 72rpx;
  background: #e0e0e0;
  border-radius: 6rpx;
  text-align: center;
}
.delete .options .opt.del {
  background: #fa6400;
  color: #fff;
}
