.flex {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.text-overflow {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
nav {
  z-index: -1;
}
.userInfo {
  width: 750rpx;
  position: relative;
  box-sizing: border-box;
  padding: 185rpx 0 30rpx;
}
.userInfo .bg {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.userInfo .user {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-bottom: 28rpx;
}
.userInfo .user .feature {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.userInfo .user .feature .icon {
  width: 54rpx;
  height: 54rpx;
  position: absolute;
  left: 110rpx;
  bottom: 0;
}
.userInfo .user .feature .avatar {
  width: 160rpx;
  height: 160rpx;
  border-radius: 80rpx;
  border: 2rpx solid #ffffff;
  margin-right: 32rpx;
  flex-shrink: 0;
}
.userInfo .user .feature .infos .name {
  max-width: 440rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 60rpx;
  margin-right: 24rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.userInfo .user .feature .infos .gender {
  width: 40rpx;
  height: 40rpx;
}
.userInfo .user .arrow {
  width: 40rpx;
  height: 40rpx;
  flex-shrink: 0;
}
.userInfo .introduction {
  max-width: 100%;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  max-height: 60rpx;
  padding: 0 50rpx 30rpx 30rpx;
  text-overflow: ellipsis;
  white-space: pre-wrap;
}
.userInfo .otherInfo {
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.userInfo .otherInfo .attention,
.userInfo .otherInfo .fans,
.userInfo .otherInfo .income {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 33rpx;
  width: 200rpx;
}
.userInfo .otherInfo .attention.income,
.userInfo .otherInfo .fans.income,
.userInfo .otherInfo .income.income {
  flex-grow: 1;
}
.userInfo .otherInfo .attention .label,
.userInfo .otherInfo .fans .label,
.userInfo .otherInfo .income .label {
  margin-bottom: 22rpx;
}
.userInfo .otherInfo .attention .num,
.userInfo .otherInfo .fans .num,
.userInfo .otherInfo .income .num {
  font-size: 72rpx;
  font-family: DINCondensed-Bold, DINCondensed;
  font-weight: bold;
  color: #000000;
  line-height: 86rpx;
}
.userInfo .otherInfo .attention .icon,
.userInfo .otherInfo .fans .icon,
.userInfo .otherInfo .income .icon {
  width: 36rpx;
  height: 36rpx;
}
.userInfo .otherInfo .attention.income .label,
.userInfo .otherInfo .fans.income .label,
.userInfo .otherInfo .income.income .label {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.userInfo .otherInfo .attention.income .label .icon,
.userInfo .otherInfo .fans.income .label .icon,
.userInfo .otherInfo .income.income .label .icon {
  margin-left: 12rpx;
}
.container .tabs {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  background: #fff;
  padding: 28rpx 0 30rpx;
  box-sizing: border-box;
  transition: all 0.3s;
  z-index: 999999;
}
.container .tabs .tab {
  margin-right: 72rpx;
  position: relative;
}
.container .tabs .tab .tabname {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000000;
  line-height: 36rpx;
  margin-bottom: 6rpx;
}
.container .tabs .tab .line {
  width: 62rpx;
  height: 10rpx;
  background: #fccc49;
  border-radius: 5rpx;
  position: absolute;
  left: 0;
  bottom: -10rpx;
}
.container .waterfall .box .column {
  width: 335rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
  align-items: flex-start;
  float: left;
}
.container .waterfall .box .column:nth-of-type(odd) {
  margin-right: 20rpx;
}
.container .waterfall .box .column .good {
  width: 100%;
  margin-bottom: 20rpx;
  background: #ffffff;
  box-shadow: 0rpx 2rpx 24rpx -6rpx rgba(186, 186, 186, 0.3);
  border-radius: 10rpx;
  position: relative;
}
.container .waterfall .box .column .good .status {
  height: 44rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 999;
}
.container .waterfall .box .column .good .video_icon {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  right: 16rpx;
  top: 16rpx;
  z-index: 9999;
}
.container .waterfall .box .column .good .imgBox {
  width: 335rpx;
  overflow: hidden;
  height: auto;
  min-height: 268rpx;
  max-height: 446rpx;
  position: relative;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx 10rpx 0 0;
}
.container .waterfall .box .column .good .imgBox .goodimg {
  width: 335rpx;
  min-width: 335rpx;
  min-height: 268rpx;
  max-height: 446rpx;
  overflow: hidden;
  flex-shrink: 0;
}
.container .waterfall .box .column .good .name {
  width: 315rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 38rpx;
  margin-bottom: 28rpx;
  margin-left: 20rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .waterfall .box .column .good .name.violation {
  color: #999999;
}
.container .waterfall .box .column .good .author {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.container .waterfall .box .column .good .author .human {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 70rpx;
}
.container .waterfall .box .column .good .author .human .imgbox {
  position: relative;
  width: 30rpx;
  height: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .waterfall .box .column .good .author .human .imgbox image {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}
.container .waterfall .box .column .good .author .human .imgbox .icon {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right: -4rpx;
  bottom: -4rpx;
  z-index: 9999;
}
.container .waterfall .box .column .good .author .human text {
  max-width: 150rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.85);
  line-height: 26rpx;
}
.container .waterfall .box .column .good .author .operate {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .waterfall .box .column .good .author .operate.cantvote text {
  color: #666666;
}
.container .waterfall .box .column .good .author .operate image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 8rpx;
}
.container .waterfall .box .column .good .author .operate text {
  font-size: 18rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 24rpx;
}
.container .waterfall .box .column .good .overlay {
  width: 335rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 10rpx 10rpx 0px 0px;
  z-index: 888;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 28rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.container .waterfall .box .column .good .overlay image {
  z-index: 999;
  width: 54rpx;
  height: 54rpx;
  margin-bottom: 12rpx;
}
.container .waterfall .box .column .good .seeMount {
  min-width: 84rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 15rpx;
  text-align: center;
  position: absolute;
  left: 16rpx;
  bottom: 16rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: center;
}
.container .waterfall .box .column .good .seeMount .icon {
  width: 20rpx;
  height: 20rpx;
  margin-right: 4rpx;
  vertical-align: middle;
}
.container .waterfall .box .column .good .seeMount .txt {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}
.container .cook-no {
  width: 100%;
  text-align: center;
}
.container .cook-no image {
  width: 300rpx;
  height: 300rpx;
}
.container .cook-no text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
}
