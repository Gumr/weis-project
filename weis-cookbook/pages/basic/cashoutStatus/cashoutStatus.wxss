.status {
  width: 80%;
  text-align: center;
  margin: 226rpx auto 296rpx;
}
.status .loading {
  width: 91rpx;
  height: 95rpx;
}
.status image {
  width: 169rpx;
  height: 169rpx;
}
.status .title {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000000;
  line-height: 36rpx;
  margin-bottom: 30rpx;
  margin-top: 48rpx;
  justify-content: center;
}
.status .tip {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 36rpx;
  text-align: center;
}
.status .uploadStatus_btn {
  width: 420rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: linear-gradient(315deg, #fb892d 0%, #fccc49 100%);
  border-radius: 44rpx;
  margin: 464rpx auto 0;
  color: #fff;
}
