.flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.text-overflow {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
page {
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
  margin: 0;
  background: #fff;
  padding: 0;
}
.container .content {
  padding: 0 30rpx;
  box-sizing: border-box;
}
.container .carousel {
  width: 690rpx;
  height: 300rpx;
  margin: 32rpx auto 50rpx;
}
.container .carousel .swiper-item {
  background: #ccc;
  border-radius: 20rpx;
}
.container .carousel .swiper-item image {
  width: 100%;
  height: 100%;
}
.container .carousel block {
  width: 100%;
}
.container .leaderboard {
  width: 100%;
  overflow: hidden;
}
.container .leaderboard .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.container .leaderboard .title .name {
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 40rpx;
}
.container .leaderboard .title .name .txt {
  margin-right: 8rpx;
}
.container .leaderboard .title .name .icon {
  width: 132rpx;
  height: 34rpx;
}
.container .leaderboard .title .opt {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
}
.container .leaderboard .title image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 8rpx;
}
.container .leaderboard .goodsCarousel {
  width: 750rpx;
  height: 270rpx;
  border-bottom: 1rpx solid #EEEEEE;
  margin-bottom: 40rpx;
}
.container .leaderboard .goodsCarousel .swiper-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  justify-content: flex-start;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.container .leaderboard .goodsCarousel .swiper-item .li {
  width: 158rpx;
  text-align: center;
  margin-right: 20rpx;
  border-top-right-radius: 10rpx;
}
.container .leaderboard .goodsCarousel .swiper-item .li .imgbox {
  width: 158rpx;
  height: 158rpx;
  overflow: hidden;
  position: relative;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}
.container .leaderboard .goodsCarousel .swiper-item .li .imgbox image {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 10rpx;
}
.container .leaderboard .goodsCarousel .swiper-item .li .name {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.85);
  line-height: 24rpx;
  max-width: 168rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .tabs {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  background: #fff;
  justify-content: flex-start;
  padding: 28rpx 30rpx 30rpx;
  box-sizing: border-box;
  transition: all 0.3s;
  z-index: 999999;
}
.container .tabs .tab {
  margin-right: 68rpx;
  position: relative;
}
.container .tabs .tab .tabname {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000000;
  line-height: 36rpx;
  margin-bottom: 6rpx;
}
.container .tabs .tab .line {
  width: 62rpx;
  height: 10rpx;
  background: #fccc49;
  border-radius: 5rpx;
  position: absolute;
  left: 50%;
  bottom: -10rpx;
  transform: translateX(-50%);
}
.container .waterfall {
  padding: 0 30rpx;
  box-sizing: border-box;
  text-align: center;
  padding-bottom: 140rpx;
}
.container .waterfall .box .column {
  width: 335rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  align-items: flex-start;
  float: left;
}
.container .waterfall .box .column:nth-of-type(odd) {
  margin-right: 20rpx;
}
.container .waterfall .box .column .good {
  width: 100%;
  height: auto;
  margin-bottom: 20rpx;
  background: #ffffff;
  box-shadow: 0rpx 2rpx 24rpx -6rpx rgba(186, 186, 186, 0.3);
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
}
.container .waterfall .box .column .good .status {
  height: 44rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 999;
}
.container .waterfall .box .column .good .video_icon {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  right: 16rpx;
  top: 16rpx;
  z-index: 8888;
}
.container .waterfall .box .column .good .imgBox {
  width: 335rpx;
  overflow: hidden;
  height: auto;
  min-height: 268rpx;
  max-height: 446rpx;
  position: relative;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx 10rpx 0 0;
}
.container .waterfall .box .column .good .imgBox .goodimg {
  width: 335rpx;
  min-width: 335rpx;
  min-height: 268rpx;
  max-height: 446rpx;
  overflow: hidden;
  flex-shrink: 0;
}
.container .waterfall .box .column .good .name {
  width: 315rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 38rpx;
  margin-bottom: 8rpx;
  margin-left: 20rpx;
  text-align: left;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.container .waterfall .box .column .good .name.violation {
  color: #999999;
}
.container .waterfall .box .column .good .author {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.container .waterfall .box .column .good .author .human {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 70rpx;
}
.container .waterfall .box .column .good .author .human .imgbox {
  position: relative;
  width: 30rpx;
  height: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .waterfall .box .column .good .author .human .imgbox image {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}
.container .waterfall .box .column .good .author .human .imgbox .icon {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right: -4rpx;
  bottom: -4rpx;
  z-index: 9999;
}
.container .waterfall .box .column .good .author .human text {
  max-width: 110rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.85);
  line-height: 26rpx;
  padding-left: 8rpx;
}
.container .waterfall .box .column .good .author .operate {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .waterfall .box .column .good .author .operate image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 8rpx;
}
.container .waterfall .box .column .good .author .operate text {
  font-size: 18rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 24rpx;
}
.container .waterfall .box .column .good .overlay {
  width: 335rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 10rpx 10rpx 0px 0px;
  z-index: 888;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 28rpx;
}
.container .waterfall .box .column .good .overlay image {
  z-index: 999;
  width: 54rpx;
  height: 54rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.container .waterfall .box .column .good .overlay text {
  position: absolute;
  left: 50%;
  top: 62%;
  transform: translate(-50%, -50%);
}
