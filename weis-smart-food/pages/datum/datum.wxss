  font-family: 'DINCondensedC';
  font-size: 52rpx;
}
.number-40 {
  font-family: 'DINCondensedC';
  font-size: 40rpx;
}
.number-28 {
  font-family: 'DINCondensedC';
  font-size: 28rpx;
}
.number-22 {
  font-family: 'DINCondensedC';
  font-size: 22rpx;
}
.number_icon {
  width: 24rpx;
  height: 24rpx;
}
.login-view {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 101;
}
