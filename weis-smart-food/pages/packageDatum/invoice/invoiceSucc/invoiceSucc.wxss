.succ {
  padding-top: 86rpx;
}
.succ .head {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 158rpx;
}
.succ .head image {
  width: 180rpx;
  height: 180rpx;
  margin-bottom: 34rpx;
}
.succ .head .status {
  color: #000000;
  font-size: 40rpx;
  font-weight: bold;
  text-align: center;
}
.succ .btn-box {
  display: flex;
  justify-content: center;
  align-items: center;
}
.succ .btn-box .btn {
  width: 280rpx;
  height: 88rpx;
  border-radius: 44rpx;
  border: 2rpx solid #999999;
  text-align: center;
  line-height: 84rpx;
  color: #999999;
  font-size: 32rpx;
}
.succ .btn-box .btn:first-of-type {
  margin-right: 20rpx;
}
.succ .btn-box .btn.fail {
  background: linear-gradient(135deg, #FF9722 0%, #FE5E0F 100%);
  border: none;
  line-height: 88rpx;
  color: #fff;
}
