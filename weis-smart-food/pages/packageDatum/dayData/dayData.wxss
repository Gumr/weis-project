page {
  background: #fbfbfb;
}
.category-unions {
  transition: height 0.2s;
  overflow: hidden;
}
.unions-card {
  overflow: hidden;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
}
.category-union {
  padding: 40rpx 30rpx;
  min-height: 430rpx;
  box-sizing: border-box;
}
.category-union--dark {
  padding-bottom: 0;
  background-color: #333333;
}
.category-union__title {
  color: #fff;
  font-size: 26rpx;
  font-weight: bold;
  margin-bottom: 40rpx;
}
.category-union__chart {
  padding-bottom: 30rpx;
}
.category-union__line {
  width: 100%;
  height: 2rpx;
  color: #454545;
}
.category-union__icon {
  width: 44rpx;
  height: 44rpx;
}
.category-union__more {
  padding: 10rpx 0;
  text-align: center;
}
.dark-background {
  background-color: #333333 !important;
}
.category-list {
  margin-left: 30rpx;
  display: flex;
  align-items: center;
}
.category-list .category {
  line-height: 20rpx;
  font-size: 18rpx;
  margin-right: 20rpx;
  color: #999;
  white-space: nowrap;
}
.category-list .category__icon {
  vertical-align: middle;
  margin-left: 4rpx;
  width: 20rpx;
  height: 20rpx;
}
.cladar {
  z-index: 1;
  width: 750rpx;
  height: 244rpx;
  background: #fff;
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(0, 0, 0, 0.05);
}
.cladar .month {
  color: #333333;
  font-size: 28rpx;
  padding-left: 30rpx;
}
.everyday {
  padding: 264rpx 0 80rpx;
  background: #fbfbfb;
}
.everyday .small-box {
  width: 690rpx;
  background: #ffffff;
  box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(164, 180, 183, 0.3);
  border-radius: 10rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 40rpx 30rpx;
  margin-bottom: 30rpx;
}
.everyday .small-box:last-child {
  margin-bottom: 0;
}
.everyday .small-box .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.everyday .small-box .title .title-left {
  display: flex;
  align-items: center;
}
.everyday .small-box .title .title-left .t-sub {
  color: #333333;
  font-size: 26rpx;
  font-weight: 550;
}
.everyday .small-box .title .title-left .tag {
  width: 104rpx;
  height: 32rpx;
  background: #ffb93f;
  border-radius: 16rpx;
  text-align: center;
  line-height: 32rpx;
  margin-left: 9rpx;
  color: #ffffff;
  font-size: 20rpx;
}
.everyday .small-box .title .share {
  width: 28rpx;
  height: 28rpx;
}
.everyday .small-box .name {
  color: #a9a9a9;
  font-size: 20rpx;
  margin: 37rpx 0 20rpx 0;
}
.everyday .small-box .name.all-xh {
  margin-top: 51rpx;
}
.everyday .small-box .data {
  display: flex;
  align-items: center;
  font-family: 'DINCondensedC';
  color: #333333;
}
.everyday .small-box .data .act {
  color: #fe5e0f;
  font-size: 44rpx;
}
.everyday .small-box .data .plan {
  display: flex;
  align-items: flex-end;
}
.everyday .small-box .data .plan .p-data {
  font-size: 44rpx;
}
.everyday .small-box .data .plan .unit {
  font-size: 24rpx;
}
.everyday .small-box .data-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.everyday .small-box .plan-xh {
  display: flex;
  align-items: flex-end;
  color: #fe5e0f;
  font-family: 'DINCondensedC';
  margin-top: 20rpx;
}
.everyday .small-box .plan-xh.color {
  color: #333333;
}
.everyday .small-box .plan-xh.right {
  justify-content: flex-end;
}
.everyday .small-box .plan-xh .p-data {
  font-size: 44rpx;
}
.everyday .small-box .plan-xh .unit {
  font-size: 24rpx;
}
.everyday .small-box .y-box {
  display: flex;
  justify-content: space-between;
}
.everyday .small-box .y-box .xh-box .name-xh {
  color: #a9a9a9;
  font-size: 22rpx;
  margin-bottom: 20rpx;
}
.everyday .small-box .progress {
  width: 630rpx;
  height: 10rpx;
  background: #eeeeee;
  border-radius: 13rpx;
  margin: 40rpx auto 60rpx;
}
.everyday .small-box .per {
  color: #333333;
  font-size: 26rpx;
  font-weight: 550;
  margin-bottom: 40rpx;
}
.everyday .small-box .per-wrap {
  display: flex;
}
.everyday .small-box .per-wrap .per-box:first-of-type {
  margin-right: 147rpx;
}
.everyday .small-box .per-wrap .per-box .per-name {
  color: #a9a9a9;
  font-size: 22rpx;
  margin-bottom: 20rpx;
}
.everyday .small-box .per-wrap .per-box .per-value {
  color: #fe5e0f;
  font-size: 44rpx;
  font-family: 'DINCondensedC';
}
.everyday .small-box .per-wrap .per-box .per-value.color {
  color: #333333;
}
.shadow {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
}
.shadow .share-box {
  width: 690rpx;
  height: 1109rpx;
  background: linear-gradient(180deg, #ff9722 0%, #fe5e0f 100%);
  border-radius: 10rpx;
}
.shadow .share-box .c-box {
  width: 630rpx;
  height: 791rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 30rpx auto 0;
}
.shadow .share-box .c-box .img {
  width: 630rpx;
  height: 358rpx;
}
.shadow .share-box .c-box .c-b {
  display: flex;
  align-items: center;
  color: #333333;
  font-weight: 550;
  margin: 84rpx 0 0 30rpx;
  z-index: 10;
}
.shadow .share-box .c-box .c-b .name {
  font-size: 24rpx;
  margin-right: 10rpx;
}
.shadow .share-box .c-box .c-b .value {
  font-size: 28rpx;
}
.shadow .share-box .c-box .c-b.line-color {
  position: relative;
}
.shadow .share-box .c-box .c-b.line-color::after {
  display: block;
  content: '';
  width: 222rpx;
  height: 12rpx;
  background: #ffd441;
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
  z-index: -1;
}
.shadow .share-box .c-box .c-b.w::after {
  width: 267rpx;
}
.shadow .share-box .c-box .c-b.c::after {
  width: 378rpx;
}
.shadow .share-box .c-box .consu {
  display: flex;
  padding: 0 0 0 30rpx;
  margin-top: 40rpx;
}
.shadow .share-box .c-box .consu.fir {
  margin-top: 44rpx;
}
.shadow .share-box .c-box .consu .consu-box:first-of-type {
  width: 315rpx;
}
.shadow .share-box .c-box .consu .consu-box .name {
  color: #a9a9a9;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}
.shadow .share-box .c-box .consu .consu-box .val {
  color: #333333;
  font-size: 28rpx;
  font-weight: 550;
}
.shadow .share-box .c-box .grey {
  width: 570rpx;
  height: 99rpx;
  background: #fbfbfb;
  box-sizing: border-box;
  padding: 45rpx 0 0 82rpx;
  position: relative;
  margin: 60rpx 0 0 30rpx;
  color: #a9a9a9;
  font-size: 24rpx;
}
.shadow .share-box .c-box .grey image {
  width: 22rpx;
  height: 16rpx;
  position: absolute;
  left: 30rpx;
  top: 30rpx;
}
.shadow .share-box .b-box {
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  margin: 30rpx auto;
}
.shadow .share-box .b-box .l-box {
  display: flex;
  align-items: center;
}
.shadow .share-box .b-box .l-box .date-b {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 74rpx;
  height: 74rpx;
}
.shadow .share-box .b-box .l-box .date-b image {
  width: 74rpx;
  height: 74rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.shadow .share-box .b-box .l-box .date-b .year,
.shadow .share-box .b-box .l-box .date-b .month {
  color: #fff;
  font-size: 20rpx;
  font-weight: 550;
}
.shadow .share-box .b-box .l-box .date-b .month {
  margin-top: 10rpx;
}
.shadow .share-box .b-box .l-box .ce-box {
  margin-left: 30rpx;
  color: #ffffff;
}
.shadow .share-box .b-box .l-box .ce-box .name {
  font-size: 32rpx;
  font-weight: 550;
  margin-bottom: 20rpx;
}
.shadow .share-box .b-box .l-box .ce-box .sb {
  font-size: 22rpx;
}
.shadow .share-box .b-box .code {
  width: 110rpx;
  height: 110rpx;
}
.shadow .share-box .btn {
  width: 630rpx;
  height: 88rpx;
  background: linear-gradient(180deg, #ffe900 0%, #ff9800 100%);
  box-shadow: 0rpx 6rpx 14rpx 0rpx #fe5e0f;
  border-radius: 44rpx;
  margin: 30rpx auto 0;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #333333;
  font-size: 30rpx;
  font-weight: 550;
}
.sign {
