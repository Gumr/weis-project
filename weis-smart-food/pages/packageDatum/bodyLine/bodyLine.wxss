.flex{display:flex}.flex-items-center{display:flex;align-items:center}.flex-content-between{display:flex;justify-content:space-between}.tabs{margin:30rpx 0 50rpx;width:360rpx;height:52rpx;background:#ebebeb;border-radius:26rpx;border:2rpx solid #ebebeb;position:relative}.tabs .tab{width:120rpx;height:52rpx;line-height:52rpx;font-size:28rpx;color:#66666 66;position:relative;z-index:1;text-align:center;transition:color .3s}.tabs .tab--active{color:#ffffff}.tabs .slide-tab{width:120rpx;height:52rpx;border-radius:26rpx;background:#fa6400;position:absolute;left:0;top:0;transition:left .3s}.dropdown-icon{padding:35rpx 35rpx 15rpx;width:44rpx;height:44rpx}.querys{padding:50rpx;padding-bottom:120rpx}.querys .query{height:100rpx;border-top:2rpx solid rgba(151,151,151,0.3);font-size:28rpx;color:#333;padding:0 10rpx}.querys .query--active{font-size:32rpx;color:#fa6400}.querys .query__icon{width:38rpx;height:38rpx}.querys .query:last-child{border-bottom:2rpx solid rgba(151,151,151,0.3)}.header{width:100%;padding:0 30rpx;box-sizing:border-box}.header__title{font-weight:bold;color:#333;font-size:28rpx}.header__icon{margin-left:20rpx;width:32rpx;height:32rpx}.header__label{font-size:24rpx;color:#333}.chart{width:750rpx;height:820rpx}.placeholder{display:flex;align-items:center;justify-content:center}.placeholder__image{width:320rpx;height:240rpx}.placeholder__label{color:#d3d3d3;font-size:30rpx;text-align:center}