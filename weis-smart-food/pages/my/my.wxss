.border {
  display: block;
  content: "";
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
page {
  background: #f4f4f4;
}
.my {
  background: #f3f4f5;
  position: relative;
}
.my .login-view {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 101;
}
.my .head {
  width: 750rpx;
  box-sizing: border-box;
  padding-top: 183rpx;
}
.my .head .bg {
  position: absolute;
  width: 750rpx;
  height: 524rpx;
  left: 0;
  top: 0;
  z-index: 1;
}
.my .head .noLogin {
  position: relative;
  z-index: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx 30rpx 0 60rpx;
}
.my .head .noLogin .col-l {
  display: flex;
  align-items: center;
}
.my .head .noLogin .col-l .icon {
  width: 140rpx;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 30rpx;
  border-radius: 50%;
  position: relative;
}
.my .head .noLogin .col-l .icon image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.my .head .noLogin .col-l .btn {
  font-size: 36rpx;
  color: #ffffff;
  font-weight: 550;
}
.my .head .noLogin .arrow {
  width: 38rpx;
  height: 38rpx;
  justify-self: flex-end;
}
.my .head .noLogin .arrow image {
  width: 100%;
  height: 100%;
}
.my .head .login {
  position: relative;
  z-index: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding-right: 30rpx;
}
.my .head .login .wrap_login {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx 0rpx 0 30rpx;
}
.my .head .login .wrap_login .icon {
  width: 140rpx;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 30rpx;
  border-radius: 50%;
  position: relative;
}
.my .head .login .wrap_login .icon image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.my .head .login .wrap_login .icon .subIcon {
  width: 70rpx;
  height: 24rpx;
  border-radius: initial;
  position: absolute;
  right: 0;
  bottom: 0;
}
.my .head .login .wrap_login .icon .crownHead {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.my .head .login .wrap_login .col-r {
  display: flex;
  flex-direction: column;
}
.my .head .login .wrap_login .col-r .nick {
  color: #ffffff;
  font-size: 36rpx;
  font-weight: 550;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.my .head .login .arrow {
  width: 28rpx;
  height: 28rpx;
}
.my .head .login .arrow.switch {
  width: 136rpx;
  height: 40rpx;
  background: #F7B500;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.my .head .login .arrow.switch image {
  width: 20rpx;
  height: 20rpx;
  margin-right: 4rpx;
}
.my .head .login .arrow.switch view {
  color: #684c00;
  font-size: 22rpx;
}
.my .head .login .arrow image {
  width: 100%;
  height: 100%;
}
.my .head .member {
  width: 690rpx;
  background: linear-gradient(270deg, #434343 0%, #3C3C3C 100%);
  border-radius: 10rpx;
  position: relative;
  z-index: 100;
  margin: 70rpx auto 0;
  padding: 28rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.my .head .member_l .crown image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.my .head .member_l .crown text {
  font-size: 26rpx;
  font-weight: bold;
  color: #FFE5D3;
}
.my .head .member_l .word {
  font-size: 24rpx;
  color: #FFE5D3;
  margin-top: 18rpx;
}
.my .head .member_r {
  padding: 0 23rpx;
  background: linear-gradient(90deg, #FFF7F1 0%, #FFC7A0 100%);
  border-radius: 26rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #333333;
  height: 52rpx;
  line-height: 52rpx;
}
.my .head .box {
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin: 20rpx auto 0;
  position: relative;
  z-index: 100;
  width: 690rpx;
  height: 212rpx;
  background: #ffffff;
  border-radius: 10rpx;
}
.my .head .box .amount-box {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.my .head .box .amount-box .amount {
  font-family: "DINCondensedC";
  font-size: 80rpx;
  font-weight: bold;
  color: #333333;
}
.my .head .box .amount-box .name {
  font-size: 24rpx;
  vertical-align: middle;
}
.my .head .box .amount-box .name text {
  font-size: 24rpx;
  color: #666666;
}
.my .head .box .amount-box .name text:nth-of-type(2) {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  margin-left: 6rpx;
  height: 28rpx;
  background: #FA6400;
  border-radius: 0px 14rpx 14rpx 14rpx;
  padding: 0 8rpx;
}
.my .order {
  width: 690rpx;
  height: 242rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 20rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: 10;
  position: relative;
}
.my .order .o-l {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 40rpx 24rpx 0;
}
.my .order .o-l .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
}
.my .order .o-l .o-r {
  display: flex;
  justify-content: center;
  align-items: center;
}
.my .order .o-l .o-r .all {
  color: #808080;
  font-size: 24rpx;
  margin-right: 6rpx;
}
.my .order .o-l .o-r .arrow {
  width: 26rpx;
  height: 26rpx;
}
.my .order .o-b {
  width: 100%;
  padding: 40rpx 0;
  display: flex;
  justify-content: center;
}
.my .order .o-b .con {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.my .order .o-b .con image {
  width: 54rpx;
  height: 54rpx;
  margin-bottom: 20rpx;
  flex-shrink: 0;
}
.my .order .o-b .con .mz {
  color: #333333;
  font-size: 22rpx;
}
.my .order .o-b .con .dot {
  width: 12rpx;
  height: 12rpx;
  background: #ff5959;
  border: 2rpx solid #ffffff;
  border-radius: 50%;
  position: absolute;
  top: 0;
  right: 30%;
}
.my .invite-banner {
  width: 702rpx;
  height: 168rpx;
}
.my .performance {
  width: 702rpx;
  height: 226rpx;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 0 auto 20rpx;
  display: flex;
}
.my .performance .box {
  flex: 1;
  height: 100%;
  box-sizing: border-box;
  padding: 40rpx 0 0 30rpx;
  position: relative;
}
.my .performance .box:first-of-type::after {
  display: block;
  content: "";
  width: 2rpx;
  height: 146rpx;
  background: #f4f4f4;
  border-radius: 20rpx;
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.my .performance .box .title {
  color: #333333;
  font-size: 28rpx;
  font-weight: 550;
  margin-bottom: 40rpx;
}
.my .performance .box .num {
  color: #333333;
  font-size: 32rpx;
  font-weight: 550;
  margin-bottom: 24rpx;
}
.my .performance .box .b-box {
  display: flex;
  align-items: center;
}
.my .performance .box .b-box .name {
  color: rgba(51, 51, 51, 0.5);
  font-size: 22rpx;
  margin-right: 10rpx;
}
.my .performance .box .b-box image {
  width: 30rpx;
  height: 30rpx;
}
.my .wrap {
  width: 690rpx;
  padding-bottom: 40rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 0 auto;
}
.my .wrap .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
  padding: 40rpx 0 0 24rpx;
}
.my .wrap .list-box {
  display: flex;
  flex-wrap: wrap;
}
.my .wrap .list-box .list {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  box-sizing: border-box;
  width: 25%;
  margin-top: 60rpx;
}
.my .wrap .list-box .list:nth-of-type(1),
.my .wrap .list-box .list:nth-of-type(2),
.my .wrap .list-box .list:nth-of-type(3),
.my .wrap .list-box .list:nth-of-type(4) {
  margin-top: 40rpx;
}
.my .wrap .list-box .list .icon {
  width: 44rpx;
  height: 44rpx;
  margin-bottom: 18rpx;
}
.my .wrap .list-box .list .name-l {
  color: #333333;
  font-size: 22rpx;
}
.my .wrap .list-box .list .have {
  width: 76rpx;
  height: 32rpx;
  background: #ff5959;
  border-radius: 16rpx 16rpx 16rpx 0rpx;
  border: 2rpx solid #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #ffffff;
  font-size: 20rpx;
  position: absolute;
  right: 0;
  top: -16rpx;
}
.my .wrap .list-box .list.invite .tag {
  width: 117rpx;
  height: 28rpx;
  background: #FA6400;
  border-radius: 0rpx 14rpx 14rpx 14rpx;
  text-align: center;
  line-height: 28rpx;
  color: #FFFFFF;
  font-size: 20rpx;
  position: absolute;
  top: 0rpx;
  left: 112rpx;
}
.my .wrap .dialog-content {
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  line-height: 50rpx;
  text-align: center;
  padding-bottom: 30rpx;
}
.version {
  color: rgba(0, 0, 0, 0.6);
  text-align: center;
  margin: 30rpx 0;
}
