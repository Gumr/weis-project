/* pages/sport/searchSport/index.wxss */
.pageauto {
  width: 100%;
}

/* pages/searchSport/index.wxss */
.popup {
  width: 540rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  padding: 104rpx 34rpx 70rpx;
  z-index: 9999 !important;
  text-align: center;
}

.popup p {
  font-size: 36rpx;
  font-weight: 400;
  color: rgba(21, 21, 23, 1);
  margin-bottom: 123rpx;
  padding: 0 36rpx;
}

.pop-btn {
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
  float: left;
  border: 1px solid #332ee6;
  margin-top: 90rpx;
}

.cancel {
  color: #332ee6;
}

.exit {
  color: #332ee6;
}


.history {
  margin-top: 47rpx;
}

.coach-tit {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 36rpx 36rpx;
}

.coach-tit span {
  font-size: 36rpx;
  font-family: Source Han Sans CN;
  font-weight: bold;
  color: rgba(21, 21, 23, 1);
}

.coach-tit image {
  width: 42rpx;
  height: 42rpx;
}

.listdiv {
  padding: 0 36rpx 36rpx;
}

.listdiv view {
  display: inline-block;
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: rgba(21, 21, 23, 1);
  margin-right: 24rpx;
  background: #f5f7fb;
  padding-left: 35rpx;
  padding-right: 35rpx;
  padding-top: 12rpx;
  padding-bottom: 12rpx;
  border-radius: 24px;
  margin-top: 36rpx;
}

.fnt {
  font-weight: bold;
}

.gray {
  background: hsla(0, 0%, 93%, 1) !important;
}

.tlt {
  text-align: center !important;
}

.i-row {
  padding-top: 40rpx;
  padding-bottom: 10rpx;
}

.demo-col {
  font-size: 26rpx;
  width: 70% !important;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  text-align: left;
  padding-left: 49rpx;
}

.demo-row {
  padding-top: 46rpx;
  padding-bottom: 10rpx;
  display: flex;
}

.imgcho {
  width: 48rpx;
  height: 48rpx;
}

.choimg {
  text-align: right;
  padding-right: 35rpx;
  width: 30% !important;
}

/* 搜索框样式 */
.search {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 80%;
  height: 70rpx;
  background: rgba(245, 247, 251, 1);
  margin-top: 40rpx;

  border-radius: 35rpx;
  margin-left: 36rpx;
  margin-right: 36rpx;

}

.mr {
  position: absolute;
  right: 30rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(169, 169, 169, 1);
}

.sericon {
  margin-left: 25rpx;
}

/* 搜索框提示文字样式 */
.search input {
  font-size: 30rpx;
  padding-left: 30rpx;
  width: 100%;
}

.fixedtab {
  position: fixed;
  z-index: 99999;
  width: 100%;
  top: 10;
}

.mr502 {
  margin-top: 40px;
  margin-bottom: 200rpx;
}

.mr50 {
  margin-top: 40px;
}

.action-replace {
  background: rgba(255, 255, 255, 1);
  z-index: 9999;
}



.replace-tit {
  height: 120rpx;
  line-height: 120rpx !important;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);

  box-sizing: border-box;
  position: relative;


}

.replace-detail {
  width: 590rpx;
  margin: 0 auto;
  line-height: 88rpx;

}

.detail-type {

  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border: 1rpx solid rgba(233, 233, 233, 1);
  background: rgba(233, 233, 233, 0.5);
  margin-bottom: 30rpx;
}

.detail-type input {
  height: 100rpx;
  text-align: left;
  width: 80%;
}

.detail-type span:nth-of-type(1) {
  font-size: 36rpx;
  font-weight: bold;
}

.detail-type span:nth-of-type(2) {
  font-size: 36rpx;
  color: rgba(102, 102, 102, 1);
}

.detail-type2 {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(168, 168, 168, 1);
  text-align: center;
  margin-top: 18rpx;
}

.detail-cnt {
  width: 678rpx;
  height: 400rpx;
  background: #f6f6f8;
  position: absolute;
  left: 50%;
  top: 120rpx;
  transform: translateX(-50%);
  border-radius: 10rpx;
}

.column {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  text-align: right;
  flex: 2;
}

.column view {
  padding-right: 34rpx;
}

.column2 view {

  padding-left: 20rpx;

}

.column3 {
  flex: 2;
  text-align: right;
}

.column3 view {
  padding-right: 64rpx;
}

.picker-box,
.picker-box2 {
  height: 80rpx;
  color: #ff5353;
  border-top: 1px solid transparent;
  border-bottom: 1px solid transparent;
  border-top: 1px solid #ff5353;
  border-bottom: 1px solid #ff5353;
}

.cds {
  width: 360rpx;
  height: 92rpx;
  background:#FE5E0FFF;
  color: #fff;
  font-size: 36rpx;
  border-radius: 40px;
  margin-top: 37rpx;
  margin-bottom: 42rpx;
}

.blue {
  color:#FE5E0FFF;
  font-weight: bold;
}

.action-no {
  margin: 120rpx auto 0;
  text-align: center;
}

.action-no image {
  width: 320rpx;
  height: 240rpx;
  margin-bottom: 48rpx;
}

.action-no span {
  display: block;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(168, 168, 168, 1);
}

.action-no button {
  width: 300rpx;
  height: 80rpx;
  border: 3rpx solid rgba(21, 21, 23, 1);
  border-radius: 40rpx;
  font-size: 30rpx;
  color: rgba(21, 21, 23, 1);
  background-color: #fff;
  margin-top: 48rpx;
}

.cate-box {
  padding: 0;
  justify-content: center;
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  left: 0;
  top: 0;
  z-index: 10003;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

.btns {
  padding: 0 50rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  margin-top: 52rpx;
  margin-bottom: 44rpx;
}

.btns .cancel {
  background: #ffffff;
  margin-right: 20rpx;


}

.btns view {
  width: 285rpx;
  height: 88rpx;
  line-height: 88rpx !important;
  text-align: center;
  border: 2rpx solid #e9e9e9;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 600;
  color: #333333;
}

.btns .sure {
  color: #ffffff;
  background: #FE5E0FFF;
  border: none !important;

}