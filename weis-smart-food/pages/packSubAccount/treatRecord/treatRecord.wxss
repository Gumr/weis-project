page {
  height: 100%;
  background: #F3F4F5;
}
.treat {
  width: 100%;
}
.treat .box {
  height: 128rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 30rpx;
  position: relative;
}
.treat .box::after {
  display: block;
  content: '';
  width: 690rpx;
  height: 2rpx;
  background: #E9E9E9;
  position: absolute;
  left: 50%;
  transform: translateX(-50%) scaleY(0.5);
  bottom: 0;
}
.treat .box:last-of-type::after {
  display: none;
}
.treat .box .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
}
.treat .box .time {
  color: #808080;
  font-size: 24rpx;
  margin-top: 14rpx;
}
.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 287rpx;
}
.empty view {
  color: #999999;
  font-size: 28rpx;
}
.empty view:first-of-type {
  font-weight: bold;
  font-size: 40rpx;
  line-height: 64rpx;
  margin-bottom: 22rpx;
}
