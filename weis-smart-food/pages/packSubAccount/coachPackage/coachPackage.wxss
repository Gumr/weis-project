page {
  background: #fbfbfb;
  height: 100%;
}
.border {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.slim {
  width: 100%;
  padding-bottom: 200rpx;
}
.slim .head {
  background: linear-gradient(180deg, #FF9F00 0%, #FF6600 100%);
  box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(0, 0, 0, 0.05);
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 10;
}
.slim .head .tip-box {
  width: 690rpx;
  background: rgba(233, 233, 233, 0.2);
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 20rpx;
  margin: 0 auto;
}
.slim .head .tip-box .title {
  color: #333333;
  font-size: 26rpx;
  font-weight: 550;
  margin-bottom: 20rpx;
}
.slim .head .tip-box .tip {
  color: #808080;
  line-height: 34rpx;
  font-size: 24rpx;
}
.slim .head .calendar {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  padding: 18rpx 0 0;
}
.slim .head .calendar .cal-box {
  width: 750rpx;
  margin: 0 auto;
  white-space: nowrap;
}
.slim .head .calendar .cal-box .scroll {
  white-space: nowrap;
  padding-left: 30rpx;
}
.slim .head .calendar .cal-box .date {
  width: 120rpx;
  height: 64rpx;
  display: inline-flex;
  background: rgba(255, 255, 255, 0.2);
  border-radius: 10rpx;
  border: 2rpx solid #FFAB73;
  padding: 13rpx 0 0rpx 8rpx;
  margin-right: 30rpx;
}
.slim .head .calendar .cal-box .date .d-left {
  display: flex;
  flex-direction: column;
  color: #FFFFFF;
}
.slim .head .calendar .cal-box .date .d-left .month,
.slim .head .calendar .cal-box .date .d-left .name {
  font-size: 18rpx;
  line-height: 20rpx;
}
.slim .head .calendar .cal-box .date .day {
  color: #FFFFFF;
  font-size: 50rpx;
  line-height: 50rpx;
  margin-left: 6rpx;
  font-family: "DINCondensedC";
}
.slim .head .calendar .cal-box .date .week {
  color: #FFFFFF;
  font-size: 26rpx;
  line-height: 22rpx;
  margin: 14rpx 0 0 6rpx;
}
.slim .head .calendar .cal-box .date.active {
  background: #fff;
  border-color: #F04F00;
}
.slim .head .calendar .cal-box .date.active .d-left .month,
.slim .head .calendar .cal-box .date.active .d-left .name {
  color: #FA6400;
}
.slim .head .calendar .cal-box .date.active .day,
.slim .head .calendar .cal-box .date.active .week {
  color: #FA6400;
}
.slim .head .tip-a {
  width: 398rpx;
  box-sizing: border-box;
  padding: 20rpx;
  background: rgba(0, 0, 0, 0.6);
  color: #ffffff;
  font-size: 22rpx;
  line-height: 36rpx;
  position: absolute;
  left: 239rpx;
  top: 200rpx;
  white-space: pre-line;
  z-index: 101;
  border-radius: 10rpx;
}
.slim .head .tip-a.active {
  padding: 0;
  width: 0;
  height: 0;
  overflow: hidden;
  transition: all 0.5s ease;
}
.slim .head .tip-a::before {
  position: absolute;
  content: '';
  border-top: 10rpx transparent solid;
  border-left: 10rpx transparent solid;
  border-right: 10rpx transparent solid;
  border-bottom: 10rpx #666666 solid;
  left: 30rpx;
  top: -20rpx;
}
.slim .meal-box {
  width: 690rpx;
  background: #ffffff;
  box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(164, 180, 183, 0.2);
  border-radius: 10rpx;
  margin: 40rpx auto 0;
}
.slim .meal-box .meal {
  margin-bottom: 48rpx;
}
.slim .meal-box .meal .ct-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 22rpx 20rpx 30rpx 30rpx;
}
.slim .meal-box .meal .ct-box .title {
  color: #333333;
  font-size: 28rpx;
  font-weight: 550;
}
.slim .meal-box .meal .ct-box .status {
  display: flex;
}
.slim .meal-box .meal .ct-box .status-sub {
  height: 44rpx;
  border-radius: 23rpx;
  border: 2rpx solid #fe5e0f;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fe5e0f;
  font-size: 22rpx;
  padding: 10rpx 18rpx;
}
.slim .meal-box .meal .ct-box .status-sub:first-of-type {
  margin-right: 20rpx;
}
.slim .meal-box .meal .good-box {
  display: flex;
  padding: 40rpx 0 40rpx 37rpx;
  position: relative;
}
.slim .meal-box .meal .good-box .good-scroll {
  white-space: nowrap;
}
.slim .meal-box .meal .good-box .good-scroll .good {
  display: inline-block;
  margin-right: 30rpx;
}
.slim .meal-box .meal .good-box .good-scroll .good:last-of-type {
  margin-right: 0;
}
.slim .meal-box .meal .good-box .good-scroll .good .icon-box {
  position: relative;
  width: 172rpx;
  height: 172rpx;
  margin-bottom: 12rpx;
  border-radius: 12rpx;
  overflow: hidden;
}
.slim .meal-box .meal .good-box .good-scroll .good .icon-box__text {
  bottom: 0;
  left: 0;
  position: absolute;
  width: 100%;
  height: 36rpx;
  line-height: 36rpx;
  background: rgba(0, 0, 0, 0.5);
  font-size: 20rpx;
  color: #fff;
  text-align: center;
}
.slim .meal-box .meal .good-box .good-scroll .good .icon {
  width: 100%;
  height: 100%;
}
.slim .meal-box .meal .good-box .good-scroll .good .name {
  color: #333333;
  font-size: 20rpx;
  margin-bottom: 14rpx;
  width: 172rpx;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
  white-space: nowrap;
  -webkit-line-clamp: 1;
}
.slim .meal-box .meal .good-box .good-scroll .good .trace {
  display: flex;
  flex-direction: column;
}
.slim .meal-box .meal .good-box .good-scroll .good .trace .trace-text {
  color: #808080;
  font-size: 18rpx;
  margin-bottom: 12rpx;
  line-height: 18rpx;
}
.slim .meal-box .meal .good-box .good-scroll .good .trace .trace-text:last-of-type {
  margin-bottom: 0;
}
.slim .meal-box .meal .day-data {
  padding: 0 30rpx;
}
.slim .meal-box .meal .day-data .t-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #808080;
  font-size: 20rpx;
  margin-bottom: 24rpx;
}
.slim .meal-box .meal .day-data.active .t-box {
  transform: scaleY(0);
  height: 0;
}
.slim .meal-box .meal .day-data.active .data-box .circle {
  width: 128rpx;
  height: 128rpx;
}
.slim .meal-box .meal .day-data.active .data-box .circle .c-img image {
  width: 128rpx;
}
.slim .meal-box .meal .day-data.active .data-box .progress {
  height: 128rpx;
  margin: 0 0 0 43rpx;
  padding: 10rpx 0;
}
.slim .meal-box .meal .price {
  padding: 30rpx 22rpx 30rpx 0;
  display: flex;
  align-items: center;
  color: #FA6400;
  font-size: 20rpx;
  justify-content: flex-end;
  position: relative;
}
.slim .meal-box .meal .price::after {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 630rpx;
  bottom: 0;
}
.slim .meal-box .meal .price .price-text {
  font-size: 24rpx;
  font-weight: bold;
}
.slim .loading {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 60%;
  transform: translateY(-50%);
}
.data-box {
  width: 100%;
  display: flex;
  align-items: center;
  box-sizing: border-box;
}
.data-box .circle {
  width: 140rpx;
  height: 140rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.data-box .circle .today {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.data-box .circle .today .name {
  color: #333333;
  font-size: 18rpx;
  margin-bottom: 12rpx;
}
.data-box .circle .today .percent {
  display: flex;
  align-items: flex-end;
  color: #a9a9a9;
  font-size: 22rpx;
}
.data-box .circle .today .percent .energy {
  color: #333333;
  display: flex;
  align-items: center;
}
.data-box .circle .today .percent .energy.active {
  color: #FA6400;
}
.data-box .circle .today .percent .denom {
  font-size: 16rpx;
}
.data-box .progress {
  flex: 1;
  margin: 0 0 0 20rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.data-box .progress .pro-box {
  display: flex;
  align-items: center;
}
.data-box .progress .pro-box:nth-of-type(2) {
  margin: 24rpx 0;
}
.data-box .progress .pro-box .name {
  width: 60rpx;
  color: #333333;
  font-size: 20rpx;
}
.data-box .progress .pro-box .bar {
  width: 256rpx;
  height: 10rpx;
  border-radius: 6rpx;
  margin: 0 0 0 20rpx;
}
.data-box .progress .pro-box .num {
  flex: 1;
  color: #a9a9a9;
  font-size: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.data-box .progress .pro-box .num .acture {
  display: flex;
  align-items: center;
  color: #333333;
}
.data-box .progress .pro-box .num .acture.active {
  color: #FA6400;
}
.dialog-content {
  padding: 0 30rpx;
}
.dialog-label {
  padding: 0 30rpx;
}
.dialog-label__meal {
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}
.dialog-label__tip {
  display: flex;
  justify-content: space-between;
  font-size: 20rpx;
  color: #808080;
  margin: 38rpx 0 24rpx;
}
.sku-list {
  padding-top: 60rpx;
  display: flex;
  flex-wrap: wrap;
}
.sku-placeholder {
  width: 268rpx;
  height: 268rpx;
  position: relative;
  margin: 68rpx auto 0;
}
.sku-placeholder__image {
  width: 100%;
  height: 100%;
}
.sku-placeholder__label {
  position: absolute;
  top: 214rpx;
  left: -9rpx;
  font-size: 22rpx;
  color: #a9a9a9;
  white-space: nowrap;
}
.dialog-footer {
  padding: 10rpx 30rpx;
}
.dialog-footer__btn {
  font-size: 30rpx;
  color: #fff;
  padding: 30rpx 0;
  text-align: center;
  border-radius: 44rpx;
}
.sku {
  width: 186rpx;
  margin: 0 70rpx 60rpx 0;
}
.sku:nth-child(3n) {
  margin-right: 0;
}
.sku__image {
  width: 186rpx;
  height: 186rpx;
  margin-bottom: 20rpx;
}
.sku__name {
  font-size: 20rpx;
  color: #333;
}
.sku__label-list {
  margin: 16rpx 0;
}
.sku__label {
  font-size: 18rpx;
  color: #808080;
  margin-bottom: 12rpx;
}
.sku__label:last-child {
  margin-bottom: 0;
}
.sku__price {
  font-size: 22rpx;
  color: #FA6400;
}
.sku-footer {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.select-btn {
  width: 70rpx;
  padding: 6rpx 0;
  font-size: 20rpx;
  color: #fe5e0f;
  border-radius: 16rpx;
  text-align: center;
  border: 2rpx solid #fe5e0f;
}
.select-btn__selected {
  background-color: #fe5e0f;
  color: #fff;
}
.btn-box {
  width: 750rpx;
  height: 182rpx;
  background: #ffffff;
  display: flex;
  justify-content: center;
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
  box-sizing: border-box;
  padding: 10rpx 0 0;
}
.btn-box .btn {
  width: 690rpx;
  height: 88rpx;
  border-radius: 44rpx;
  text-align: center;
  line-height: 88rpx;
  color: #ffffff;
  font-size: 30rpx;
  font-weight: 550;
}
.btn-box .discount {
  position: absolute;
  bottom: 184rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 20rpx;
  background: #333333;
  border-radius: 20rpx;
  opacity: 0.7;
  font-size: 20rpx;
  color: #FFFFFF;
}
.btn-box .discount text:nth-child(2) {
  font-weight: bold;
  color: #FCA649;
}
.btn-box .discount .triangle {
  position: absolute;
  top: 40rpx;
  right: 46%;
  width: 0;
  height: 0;
  border: 8rpx solid transparent;
  border-top: 8rpx solid #333333;
}
.snow {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.snow .snowflake {
  width: 470rpx;
  height: 60rpx;
  background: #ff8c53;
  box-shadow: 0rpx 6rpx 18rpx 0px rgba(254, 94, 15, 0.4);
  border-radius: 36rpx;
  position: relative;
}
.snow .snowflake .load_rotate {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 4rpx;
  width: 52rpx;
  height: 52rpx;
  animation: spin 1s linear infinite;
  -webkit-animation: spin 1s linear infinite;
  z-index: 10;
}
.snow .snowflake .leaf {
  position: absolute;
  top: 14rpx;
  right: 35rpx;
  width: 19rpx;
  height: 20rpx;
  z-index: 9;
}
.snow .snowflake .leaf.leaf1 {
  animation: leaf1 3s linear infinite;
  -webkit-animation: leaf1 3s linear infinite;
}
.snow .snowflake .leaf.leaf2 {
  animation: leaf1 3s linear 1s infinite;
  -webkit-animation: leaf1 3s linear 1s infinite;
}
.snow .snowflake .leaf.leaf3 {
  animation: leaf1 3s linear 2s infinite;
  -webkit-animation: leaf1 3s linear 2s infinite;
}
.snow .snowflake .leaf.leaf4 {
  animation: leaf1 3s linear 3s infinite;
  -webkit-animation: leaf1 3s linear 3s infinite;
}
.snow .snowflake .progress-animation {
  position: absolute;
  z-index: 11;
  background: #fe5e0f;
  border-radius: 36rpx;
  height: 60rpx;
  width: 0rpx;
  animation: speed 3s linear 3s forwards;
  -webkit-animation: speed 3s linear 3s forwards;
}
.snow .snowflake .mark {
  width: 38rpx;
  height: 20rpx;
  border-left: 4rpx solid #ffffff;
  border-bottom: 4rpx solid #ffffff;
  transform: rotate(-45deg) scale(0);
  position: absolute;
  left: 0;
  right: 0;
  top: -10rpx;
  bottom: 0;
  margin: auto;
  animation: mark 0s linear 6s forwards;
  -webkit-animation: mark 0s linear 6s forwards;
  z-index: 12;
}
.snow .ready {
  color: #a9a9a9;
  font-size: 28rpx;
  margin-top: 60rpx;
  text-align: center;
}
@keyframes spin {
  0% {
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
  }
}
@keyframes leaf1 {
  0% {
    right: 35rpx;
    top: 14rpx;
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
  }
  100% {
    right: 450rpx;
    top: 18rpx;
    transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
  }
}
@keyframes speed {
  0% {
    width: 0rpx;
  }
  100% {
    width: 470rpx;
  }
}
@keyframes mark {
  0% {
    transform: scale(0);
    -webkit-transform: scale(0);
  }
  100% {
    transform: rotate(-45deg) scale(1);
    -webkit-transform: rotate(-45deg) scale(1);
  }
}
