page {
  background: #F3F4F5;
  height: 100%;
}
.treat {
  width: 100%;
  background: #F3F4F5;
}
.treat .box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 112rpx;
  padding: 0 40rpx 0 34rpx;
  position: relative;
  background: rgba(243, 244, 245, 0.57);
}
.treat .box::after {
  display: block;
  content: '';
  width: 690rpx;
  height: 2rpx;
  background: rgba(0, 0, 0, 0.1);
  position: absolute;
  left: 50%;
  transform: translateX(-50%) scaleY(0.5);
  bottom: 0;
}
.treat .box .info {
  display: flex;
}
.treat .box .info .head {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  border: 2rpx solid #FCCC49;
  margin-right: 24rpx;
}
.treat .box .info .box-col {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.treat .box .info .box-col .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
}
.treat .box .info .box-col .name image {
  width: 85rpx;
  height: 28rpx;
  margin-left: 12rpx;
}
.treat .box .info .box-col .date {
  color: #999999;
  font-size: 20rpx;
  margin-top: 8rpx;
}
.treat .box .info-right {
  display: flex;
  align-items: center;
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
}
.treat .box .info-right .price {
  text-align: right;
}
.treat .box .info-right .backPrice {
  width: 140rpx;
  text-align: right;
}
.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 287rpx;
}
.empty view {
  color: #999999;
  font-size: 28rpx;
}
.empty view:first-of-type {
  font-weight: bold;
  font-size: 40rpx;
  line-height: 64rpx;
  margin-bottom: 22rpx;
}
