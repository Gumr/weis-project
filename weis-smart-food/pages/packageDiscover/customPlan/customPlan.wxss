.bot {
  width: 750rpx;
  height: 148rpx;
  background: #ffffff;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
  padding-top: 20rpx;
}
.bot .btn {
  width: 690rpx;
  height: 88rpx;
  background: linear-gradient(180deg, #ff9722 0%, #fe5e0f 100%);
  box-shadow: 0rpx 6rpx 14rpx 0rpx rgba(254, 94, 15, 0.5);
  border-radius: 44rpx;
  color: #ffffff;
  font-size: 30rpx;
  font-weight: 550;
  display: flex;
  justify-content: center;
  align-items: center;
}
.para {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 10001;
}
.para .empty {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.para .goal {
  width: 100%;
  background: #ffffff;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  padding-bottom: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 20rpx 20rpx 0 0;
  z-index: 10;
}
.para .goal .title {
  color: #000000;
  font-size: 32rpx;
  font-weight: 550;
  text-align: center;
  margin: 35rpx auto 0rpx;
}
.para .goal .tip {
  color: #a9a9a9;
  font-size: 28rpx;
  text-align: center;
  margin: 10rpx auto 14rpx;
}
.para .goal .wrap_box {
  width: 660rpx;
  box-sizing: border-box;
  padding-top: 30rpx;
  display: flex;
}
.para .goal .wrap_box.uniq {
  height: initial;
}
.para .goal .wrap_box .box {
  width: 200rpx;
  height: 200rpx;
  background: rgba(233, 233, 233, 0.5);
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  text-align: center;
  padding: 32rpx 0;
  display: inline-block;
  margin-right: 20rpx;
}
.para .goal .wrap_box .box:nth-of-type(3n) {
  margin-right: 0;
}
.para .goal .wrap_box .box:last-of-type {
  margin-bottom: 0;
}
.para .goal .wrap_box .box .name {
  font-size: 28rpx;
  color: #000000;
  margin-bottom: 20rpx;
}
.para .goal .wrap_box .box input {
  width: 110rpx;
  height: 80rpx;
  box-sizing: border-box;
  font-size: 80rpx;
  font-family: 'DINCondensedC';
  font-weight: bold;
  color: #333333;
  display: inline-block;
}
.para .goal .wrap_box .box .ph-color {
  color: #a9a9a9;
  font-size: 28rpx;
}
.para .goal .wrap_box .box .unit {
  font-size: 46rpx;
  font-family: 'DINCondensedC';
  font-weight: bold;
  color: #333333;
  display: inline-block;
}
.para .goal .warn {
  font-size: 22rpx;
  color: #FA6400;
}
.para .goal .warn image {
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
}
.para .goal .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 52rpx auto 0;
}
.para .goal .btn .qx {
  box-sizing: border-box;
  width: 285rpx;
  height: 88rpx;
  border: 2rpx solid #e9e9e9;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #333333;
  font-size: 32rpx;
  margin-right: 19rpx;
}
.para .goal .btn .sure {
  width: 590rpx;
  height: 88rpx;
  background: linear-gradient(180deg, #ff9722 0%, #fe5e0f 100%);
  box-shadow: 0rpx 6rpx 14rpx 0rpx rgba(254, 94, 15, 0.5);
  border-radius: 44rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 0.5);
  font-weight: 550;
}
.para .goal .btn .sure.active {
  color: #ffffff;
}
.para .goal .close {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  left: 35rpx;
  top: 35rpx;
}
.para.active .goal .box {
  background: rgba(255, 89, 89, 0.1);
  border: 1rpx solid #ff5959;
}
.popupshow {
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 1;
  height: 0;
  overflow: hidden;
  background-color: #fff;
}
.popupshow.active {
  height: auto;
  overflow: visible;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.6);
}
