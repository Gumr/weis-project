.calendar-container {
  width: 100%;
  position: absolute;
  top: 0;
  z-index: 1;
  height: 0;
  overflow: hidden;
  background-color: #fff;
}
.calendar-container-active {
  height: auto;
  overflow: visible;
}
.gray-bottom-border:after {
  display: block;
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  background-color: #e9e9e9;
}
.gray-top-border:before {
  display: block;
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  background-color: #e9e9e9;
}
.display-flex {
  display: flex;
}
.detail-icon {
  width: 32rpx;
  height: 32rpx;
}
.no-card {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: -1;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.no-card image {
  width: 268rpx;
  height: 268rpx;
}
.no-card view {
  color: #a9a9a9;
  font-size: 22rpx;
  text-align: center;
}
.no-card view:first-of-type {
  margin-top: -22rpx;
}
.no-card view:last-of-type {
  margin-top: 19rpx;
}
.tab {
  width: 100%;
  height: 98rpx;
  background: #ffffff;
  box-shadow: 0rpx -6rpx 20rpx 0rpx rgba(0, 0, 0, 0.07);
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.tab.active {
  padding-bottom: 68rpx;
  box-sizing: content-box;
}
.tab .tab-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab .tab-box image {
  width: 44rpx;
  height: 44rpx;
  margin-bottom: 10rpx;
}
.tab .tab-box .meal {
  font-size: 20rpx;
  color: #333333;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.6);
}
.program {
  width: 690rpx;
  height: 326rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius: 20rpx;
  margin: 30rpx auto 0;
}
.program .sem-box {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 22rpx;
}
.program .sem-box .box-s {
  width: 158rpx;
  height: 158rpx;
}
.program .sem-box .box-s:nth-of-type(2) {
  width: 212rpx;
  height: 212rpx;
  margin: 0 53rpx;
}
.program .progress-line {
  display: flex;
  align-items: center;
  margin: 10rpx auto 0;
  justify-content: center;
}
.program .progress-line .all-energy {
  display: flex;
  align-items: flex-end;
  color: rgba(51, 51, 51, 0.7);
  font-size: 20rpx;
  margin-right: 20rpx;
}
.program .progress-line .all-energy text {
  font-size: 18rpx;
}
.program .progress-line .bar {
  width: 428rpx;
  height: 12rpx;
  background: #E9E9E9;
  border-radius: 6rpx;
  position: relative;
}
.program .progress-line .bar .dot {
  width: 12rpx;
  height: 12rpx;
  background: #F7B500;
  border-radius: 6rpx;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 10;
}
.program .progress-line .bar .dot-per {
  position: absolute;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 4rpx 0rpx #CACACA;
  border: 3rpx solid #F7B500;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #333333;
  font-size: 20rpx;
  font-family: "DINCondensedC";
  transition: all 100ms ease;
}
.program .progress-line .bar .dot-per .unit {
  font-size: 14rpx;
}
.program .progress-line .p-box {
  display: flex;
  align-items: flex-end;
  color: #333333;
  font-family: "DINCondensedC";
  line-height: 32rpx;
  margin-left: 20rpx;
}
.program .progress-line .p-box .actual {
  font-size: 36rpx;
}
.program .progress-line .p-box .plan {
  font-size: 26rpx;
}
.meal-item-wrap {
  width: 100%;
  padding: 20rpx 0 10rpx;
}
.meal-item-wrap.active {
  padding-bottom: 180rpx;
}
.meal-item-box {
  margin: 0 30rpx 0 32rpx;
}
.meal-item-header {
  padding: 44rpx 0 19rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.meal-item-header .meal-title-box {
  display: flex;
  align-items: center;
}
.meal-item-header .meal-title-box .meal-recommand {
  color: #a9a9a9;
  font-size: 24rpx;
  margin-left: 20rpx;
}
.meal-item-header .meal-title {
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 550;
}
.meal-icon {
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.meal-food-list {
  padding-bottom: 25rpx;
}
.meal-food-list .meal-food-item {
  padding: 15rpx 5rpx 15rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.meal-food-list .meal-food-item .meal-food-box {
  display: flex;
  align-items: center;
}
.meal-food-list .meal-food-item .food-image-box {
  position: relative;
  margin-left: 6rpx;
}
.meal-food-list .meal-food-item .card-head-label {
  width: 60rpx;
  height: 22rpx;
  position: absolute;
  left: -6rpx;
  top: 6rpx;
}
.meal-food-list .meal-food-item .meal-food-image {
  width: 98rpx;
  height: 98rpx;
  margin-right: 30rpx;
}
.meal-food-list .meal-food-item .meal-food-name {
  color: #333;
  font-weight: 550;
  font-size: 28rpx;
  margin-bottom: 22rpx;
}
.meal-food-list .meal-food-item .meal-food-text {
  font-size: 24rpx;
  color: #a9a9a9;
}
.meal-food-list .meal-food-item .next-icon {
  width: 24rpx;
  height: 24rpx;
  margin-left: 7rpx;
  transform: rotate(-90deg);
}
.meal-food-list .meal-food-item .food-swipe-next-box {
  flex-grow: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.meal-food-list .food-swipe-right {
  background-color: #fe5e0f;
  color: #fff;
  padding: 0 37rpx;
  height: 100%;
  display: flex;
}
.meal-food-list .food-swipe-right .food-swipe-text {
  margin: auto;
  display: inline-block;
}
.reality {
  display: flex;
  align-items: center;
}
.reality .real {
  color: #333333;
  font-size: 24rpx;
  margin-right: 10rpx;
  display: flex;
  align-items: center;
}
.reality .real text.add {
  color: #ff5959;
}
.reality .real text.reduce {
  color: #fe5e0f;
}
.reality .add-meal-icon {
  width: 28rpx;
  height: 28rpx;
  transform: rotate(-90deg);
}
.food-list-item {
  padding: 30rpx 0;
  margin: 0 30rpx;
  display: flex;
}
.food-list-item .food-image {
  width: 98rpx;
  height: 98rpx;
  margin-right: 30rpx;
}
.food-list-item .food-text-wrap {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.food-list-item .food-name {
  color: #333;
  font-size: 28rpx;
  font-weight: 700;
  margin-bottom: 20rpx;
}
.food-list-item .food-text {
  color: #a9a9a9;
  font-size: 24rpx;
}
.food-list-item .food-kcal {
  color: #ff5959;
}
.gray-bt-border {
  border-bottom: 1px solid #e9e9e9;
}
.add-food-header {
  width: 100%;
  padding: 34rpx 30rpx;
  display: flex;
  justify-content: space-between;
}
.add-food-header .add-food-action-text {
  font-size: 28rpx;
  color: #333;
}
.add-food-header .add-food-title {
  color: #333;
  font-size: 32rpx;
  font-weight: 400;
}
.add-food-header .add-text {
  color: #fe5e0f;
}
.popup-foot-item {
  align-items: center;
  justify-content: space-between;
}
.food-count-bar {
  font-size: 28rpx;
  color: #fe5e0f;
  text-align: center;
  margin-bottom: 20rpx;
}
.food-count-bar .food-count-text {
  font-size: 60rpx;
  margin-right: 6rpx;
  font-weight: bold;
}
.food-count-scale-wrap {
  padding: 0 58rpx;
}
.food-unit-bar {
  margin: 30rpx 0 40rpx 0;
  text-align: center;
}
.food-unit-bar .food-unit-text {
  font-size: 24rpx;
  color: #a9a9a9;
  display: inline-block;
  padding: 18rpx 30rpx;
}
.food-unit-bar .food-unit-text-active {
  color: #fe5e0f;
}
.padding-footer-height {
  padding-bottom: 148rpx;
}
.food-select-bar {
  height: 148rpx;
  background: #ffffff;
  box-shadow: 0 -6rpx 20rpx 0 rgba(0, 0, 0, 0.07);
  border-radius: 40rpx 40rpx 0 0;
  padding: 40rpx 30rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.food-select-bar .add-text {
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
  margin-right: 20rpx;
}
.food-select-bar .add-count {
  font-size: 28rpx;
  color: #ff5959;
  font-weight: bold;
}
.food-select-bar .total-text-wrap {
  margin-left: 40rpx;
  font-size: 28rpx;
  color: #a9a9a9;
}
.food-select-bar .food-confirm-btn {
  background-color: #fe5e0f;
  padding: 20rpx 40rpx;
  color: #fff;
  font-size: 28rpx;
}
.delete-food-bar {
  padding: 40rpx 0;
}
.delete-food-bar .delete-food-box {
  display: flex;
  align-items: center;
  justify-content: center;
}
.delete-food-bar .delete-food-icon {
  width: 28rpx;
  height: 28rpx;
}
.delete-food-bar .delete-food-text {
  font-size: 28rpx;
  margin-left: 10rpx;
  color: #a9a9a9;
}
.sport-box {
  width: 100%;
  margin-top: 40rpx;
}
.sport-box .title {
  color: #333333;
  font-size: 32rpx;
  font-weight: 550;
  margin-bottom: 40rpx;
  padding-left: 30rpx;
}
.actionlist {
  width: 100%;
  margin-bottom: 100rpx;
}
.actionlist.active {
  margin-bottom: 180rpx;
}
.actionlist .category {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 40rpx 30rpx 30rpx;
  padding-bottom: 0;
}
.actionlist .category .each {
  width: 50%;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #a9a9a9;
  line-height: 24rpx;
}
.actionlist .category .each:nth-of-type(1) {
  text-align: left;
}
.actionlist .category .each:last-of-type {
  text-align: right;
}
.actionlist .content {
  padding: 0 37rpx 0 30rpx;
  margin-bottom: 40rpx;
}
.actionlist .content .each {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 30rpx 0;
}
.actionlist .content .each view {
  width: 25%;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 24rpx;
  text-align: center;
}
.actionlist .content .each view:nth-of-type(1) {
  text-align: left;
}
.actionlist .content .each view:last-of-type {
  text-align: right;
}
.actionlist .content .each view text:nth-of-type(2) {
  font-size: 20rpx;
  color: #A9A9A9;
}
.actionlist .content .copy,
.actionlist .content .delete {
  width: 98rpx;
  height: 84rpx;
  line-height: 84rpx !important;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 24rpx;
  float: left;
  text-align: center;
}
.actionlist .content .copy.copy,
.actionlist .content .delete.copy {
  background: #21c4c0;
}
.actionlist .content .copy.delete,
.actionlist .content .delete.delete {
  background: #ff5959;
}
.sport-record-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.sport-record-item .c-l {
  display: flex;
  align-items: center;
}
.sport-record-item .c-l .icon {
  width: 98rpx;
  height: 98rpx;
  margin-right: 30rpx;
}
.sport-record-item .sport-name {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width: 240rpx;
  color: #333333;
  font-size: 28rpx;
  font-weight: 550;
  margin-bottom: 20rpx;
}
.sport-record-item .sport-text-wrap {
  display: flex;
  flex-wrap: wrap;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.sport-record-item .sport-text-wrap::after {
  content: "";
  flex: auto;
}
.sport-record-item .sport-text-item {
  color: #a9a9a9;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.sport-record-item .sport-text-item .sport-text {
  display: flex;
  align-items: center;
}
.sport-record-item .sport-text-item.sport-icon {
  display: flex;
  align-items: center;
}
.sport-record-item .sport-text-item.sport-icon .sport-text {
  margin-right: 10rpx;
}
.sport-record-item .sport-text-item.sport-icon .arrow {
  width: 28rpx;
  height: 28rpx;
}
.sport-record-item .sport-item-calorie {
  width: 40%;
}
.sport-record-item .sport-unit {
  font-size: 18rpx;
}
.empty {
  height: 100%;
  display: flex;
  padding-top: 40%;
  color: #b1b1b1;
}
