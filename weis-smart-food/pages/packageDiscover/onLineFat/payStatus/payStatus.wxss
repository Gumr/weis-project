.pay {
  margin-top: 90rpx;
}
.pay .head {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 36rpx;
}
.pay .head image {
  width: 180rpx;
  height: 180rpx;
  margin-bottom: 34rpx;
}
.pay .head .status {
  color: #000000;
  font-size: 40rpx;
  font-weight: 550;
  text-align: center;
}
.pay .btn-box {
  display: flex;
  justify-content: center;
  align-items: center;
}
.pay .btn-box .btn {
  width: 280rpx;
  height: 88rpx;
  border-radius: 44rpx;
  border: 2rpx solid #fa6400;
  text-align: center;
  line-height: 88rpx;
  color: #fa6400;
  font-size: 32rpx;
}
.pay .btn-box .btn:first-of-type {
  margin-right: 20rpx;
}
.pay .btn-box .btn.fail {
  background: linear-gradient(135deg, #ff9722 0%, #fe5e0f 100%);
  color: #fff;
}
.wechat {
