}
.wechat .c {
  width: 304rpx;
  height: 368rpx;
  background: #ffffff;
  border-radius: 5rpx;
  box-sizing: border-box;
  padding: 30rpx 0 0;
  margin: 30rpx auto 0;
}
.wechat .c .title {
  color: #808080;
  font-size: 22rpx;
  margin: 0 auto 24rpx;
  text-align: center;
}
.wechat .c image {
  width: 280rpx;
  height: 280rpx;
}
