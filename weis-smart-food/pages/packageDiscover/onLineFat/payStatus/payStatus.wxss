.logindio {
  position: relative;
}
.dialog {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 200;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
}
.closeimg {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 0;
  top: 40rpx;
}
.logintip2 {
  position: absolute;
  top: 340rpx;
