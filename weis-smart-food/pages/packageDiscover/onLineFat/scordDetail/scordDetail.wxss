.box {
  width: 100%;
  padding: 15rpx 40rpx 0;
}
.box .list {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx 0;
  font-weight: bold;
  position: relative;
}
.box .list::after {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  transform: scaleY(0.5);
  background: rgba(51, 51, 51, 0.1);
  position: absolute;
  left: 0;
  bottom: 0;
}
.box .list .date {
  color: #333333;
  font-size: 28rpx;
}
.box .list .score {
  color: #fe5e0f;
  font-size: 30rpx;
}
.null {
  margin-top: 164rpx;
  text-align: center;
}
.null image {
  width: 320rpx;
  height: 240rpx;
}
.null view {
  font-size: 28rpx;
  color: #a9a9a9;
  margin-top: 20rpx;
}
