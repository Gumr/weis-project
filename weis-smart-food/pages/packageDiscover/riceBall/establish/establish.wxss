.flex {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container {
  width: 100%;
  overflow: hidden;
}
.container .header .icon {
  width: 100vw;
  height: 200rpx;
}
.container .steps {
  background: #FFFFFF;
  border-radius: 20rpx;
  z-index: 88;
  transform: translateY(-33rpx);
  padding: 33rpx 30rpx 0;
}
.container .steps__one {
  margin-bottom: 40rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-between;
}
.container .steps__one.steps__one--cover {
  justify-content: flex-start;
}
.container .steps__one-label {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 40rpx;
}
.container .steps__one-opts {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .steps__one-opts .radio {
  margin-right: 102rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .steps__one-opts .radio .icon {
  width: 32rpx;
  height: 32rpx;
  border-radius: 16rpx;
  margin-right: 13rpx;
}
.container .steps__one-opts .radio text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 28rpx;
}
.container .steps__one .ipt-box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.container .steps__one .ipt-box input {
  min-width: 380rpx;
  height: 80rpx;
  text-align: right;
  line-height: normal;
  padding-left: 40rpx;
  box-sizing: border-box;
  color: #333333;
  margin-right: 16rpx;
}
.container .steps__one .ipt-box input::-webkit-input-placeholder {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #A9A9A9;
}
.container .steps__one .ipt-box .icon {
  width: 28rpx;
  height: 28rpx;
}
.container .steps-imgs {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-left: 24rpx;
}
.container .steps-imgs .imgs-add {
  width: 160rpx;
  height: 160rpx;
  line-height: 160rpx;
  text-align: center;
  background: #F4F4F4;
  border-radius: 10rpx;
}
.container .steps-imgs .imgs-add .icon {
  width: 80rpx;
  height: 80rpx;
  vertical-align: middle;
}
.container .steps-imgs .imgs-cover {
  position: relative;
}
.container .steps-imgs .imgs-cover .icon {
  width: 160rpx;
  height: 160rpx;
}
.container .steps-imgs .imgs-cover .icon-close {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 0;
  top: 0;
  transform: translate(50%, -50%);
}
.container .rule {
  padding-bottom: 250rpx;
}
.container .rule .tit {
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  margin-bottom: 30rpx;
}
.container .rule .icon {
  width: 690rpx;
  height: 940rpx;
  margin: 0 auto;
}
.container .operate-btns {
  position: fixed;
  left: 50%;
  bottom: 50rpx;
  transform: translateX(-50%);
  padding: 0 30rpx 0 54rpx;
}
.container .operate-btns .pre.btn {
  width: 650rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 50rpx;
  text-align: center;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  background: linear-gradient(90deg, #FB892D 0%, #FE5E0F 100%);
}
.container .operate-btns .pre.btn.disabled {
  background: #D8D8D8;
}
.container .operate-btns .next {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  justify-content: space-between;
}
.container .operate-btns .next-l image {
  width: 48rpx;
  height: 67rpx;
}
.container .operate-btns .next-r {
  width: 554rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: linear-gradient(90deg, #FB892D 0%, #FE5E0F 100%);
  border-radius: 50rpx;
}
.container .operate-btns .next-r button {
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}
.popup-operate {
  width: 750rpx;
  background: #FFFFFF;
  border-radius: 40rpx 40rpx 0px 0px;
  text-align: center;
}
.popup-operate .operate {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  border-bottom: 1rpx solid rgba(51, 51, 51, 0.1);
  padding: 60rpx 0;
}
.popup-operate .operate.edit {
  color: #FE5E0F;
}
.popup-operate .cancel {
  padding: 40rpx 0;
}
.confirmDialog {
  text-align: center;
}
.confirmDialog .title {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 44rpx;
  padding: 60rpx 0;
}
