  margin-bottom: 36rpx;
}
.welfare .coupon-center .tab {
  width: 100%;
  padding-left: 35rpx;
  margin-bottom: 20rpx;
}
.welfare .coupon-center .tab .scroll-box {
  height: 100rpx;
  display: flex;
  white-space: nowrap;
}
.welfare .coupon-center .tab .scroll-box .box {
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  color: #747474;
}
.welfare .coupon-center .tab .scroll-box .box.overRow {
  margin-right: 40rpx;
}
.welfare .coupon-center .tab .scroll-box .box.active {
  color: #fa6400;
}
.welfare .coupon-center .tab .scroll-box .box.active .status {
  width: 128rpx;
  height: 38rpx;
  background: linear-gradient(90deg, #fb892d 0%, #fe5e0f 100%);
  border-radius: 19rpx;
  text-align: center;
  line-height: 38rpx;
  color: #ffffff;
  font-weight: bold;
}
.welfare .coupon-center .tab .scroll-box .box .time {
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  font-family: "DINCondensedC";
}
.welfare .coupon-center .tab .scroll-box .box .status {
  width: 128rpx;
  height: 38rpx;
  text-align: center;
  line-height: 38rpx;
  font-size: 22rpx;
}
.welfare .coupon-center .flex {
  display: flex;
}
.welfare .coupon-center .flex-items-center {
  display: flex;
  align-items: center;
}
.welfare .coupon-center .ellipsis {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.welfare .coupon-center .tip {
  background: rgba(250, 100, 0, 0.1);
