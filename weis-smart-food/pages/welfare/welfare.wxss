.welfare .header {
  padding: 20rpx 30rpx 40rpx;
  background: #f3f4f5;
}
.welfare .header .slideshow {
  width: 690rpx;
  height: 300rpx;
  position: relative;
}
.welfare .header .slideshow__image {
  width: 690rpx;
  height: 300rpx;
  border-radius: 10rpx;
}
.welfare .header .slideshow__dots {
  position: absolute;
  bottom: 12rpx;
  left: 345rpx;
  transform: translateX(-50%);
  display: flex;
}
.welfare .header .slideshow__dot {
  width: 8rpx;
  height: 8rpx;
  background: #ffffff;
  margin-right: 6rpx;
  border-radius: 4rpx;
  display: flex;
}
.welfare .header .slideshow__dot--active {
  width: 24rpx;
  height: 8rpx;
  opacity: 0.5;
}
.welfare .header .member {
  width: 690rpx;
  height: 172rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  position: relative;
}
.welfare .header .member .crownBg {
  width: 100%;
  height: 100%;
}
.welfare .header .member .content {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  padding: 24rpx 30rpx 16rpx;
}
.welfare .header .member .content .top {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.welfare .header .member .content .top_l .head {
  display: flex;
  align-items: center;
}
.welfare .header .member .content .top_l .head .headImg {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.welfare .header .member .content .top_l .head .headName .uname {
  font-size: 28rpx;
  font-weight: bold;
  color: #644201;
}
.welfare .header .member .content .top_l .head .headName .crown {
  display: flex;
  align-items: center;
}
.welfare .header .member .content .top_l .head .headName .crown image {
  width: 30rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.welfare .header .member .content .top_l .head .headName .crown text {
  font-size: 24rpx;
  color: #8f5e00;
}
.welfare .header .member .content .top_r {
  padding: 0 23rpx;
  background: linear-gradient(90deg, #fb892d 0%, #fe5e0f 100%);
  border-radius: 26rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #fff1d6;
  height: 52rpx;
  line-height: 52rpx;
}
.welfare .header .member .content .bottom {
  font-size: 24rpx;
  color: #644201;
  margin-top: 28rpx;
}
.welfare .header .member .content .bottom text {
  font-size: 28rpx;
  font-weight: bold;
}
.welfare .header .member.vipStyle .top_r {
  background: #C68B1C;
  color: #F8DBA3;
}
.welfare .header .square-box {
  width: 690rpx;
  height: 196rpx;
  background: #ffffff;
  border-radius: 10rpx;
  margin: 30rpx auto 0;
  padding: 23rpx 26rpx 0;
  display: flex;
  justify-content: space-between;
}
.welfare .header .square-box .box {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.welfare .header .square-box .box image {
  width: 120rpx;
  height: 120rpx;
}
.welfare .header .square-box .box .name {
  color: #333333;
  font-size: 24rpx;
}
.welfare .header .square-box .box .desc {
  height: 28rpx;
  background: #FFE191;
  border-radius: 12rpx;
  line-height: 28rpx;
  font-size: 17rpx;
  color: #AD8312;
  text-align: center;
  padding: 0 8rpx;
  position: absolute;
  left: 12rpx;
  bottom: 76rpx;
}
.welfare .header .content-box {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.welfare .header .content-box .list {
  width: 218rpx;
  height: 144rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  position: relative;
}
.welfare .header .content-box .list .img {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.welfare .header .content-box .list .cont {
  padding: 20rpx;
}
.welfare .header .content-box .list .cont_name {
  font-size: 28rpx;
  font-weight: bold;
  color: #4159C0;
  line-height: 40rpx;
  margin-bottom: 6rpx;
}
.welfare .header .content-box .list .cont_desc {
  font-size: 20rpx;
  color: #8D9BD9;
  line-height: 28rpx;
}
.welfare .coupon-center {
  width: 100%;
}
.welfare .coupon-center .title {
