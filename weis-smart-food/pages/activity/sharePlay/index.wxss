page {
  background: #FFD47BFF;
  flex: unset;
}
page button {
  padding: 0;
  margin: 0;
  background: transparent;
}
page button::after {
  border: none;
}
.playcard {
  width: 100%;
}
.playcard .topbg {
  width: 100%;
  position: absolute;
  top: 0;
}
.playcard .activedetail {
  width: 60rpx;
  height: 156rpx;
  position: absolute;
  right: 0;
  top: 172rpx;
}
.playcard-poker {
  text-align: center;
  position: relative;
  margin: 160rpx auto 0;
  width: 608rpx;
  height: 858rpx;
  padding-top: 100rpx;
}
.playcard-poker .pokerImg {
  width: 608rpx;
  height: 858rpx;
