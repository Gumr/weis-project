  display: flex;
  justify-content: space-between;
}
.playcard-poker .poker__score__each text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFE0A2;
}
.playcard-poker .shareBtn {
