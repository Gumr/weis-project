  z-index: 88;
}
.playcard-poker .poker__score__total {
  font-size: 40rpx;
  line-height: 56rpx;
  font-weight: bold;
  color: #fff;
}
.playcard-poker .poker__score .heal-score {
  margin: 10rpx auto 0;
