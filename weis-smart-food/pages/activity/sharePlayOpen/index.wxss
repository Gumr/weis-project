page {
  background: #FFD47BFF;
  position: relative;
}
page button {
  padding: 0;
  margin: 0;
  background: transparent;
}
page button::after {
  border: none;
}
.errorview text:nth-child(1) {
  font-size: 28rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
}
.errorview view {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  margin-top: 12rpx;
}
.main {
  width: 562rpx;
  height: 760rpx;
  perspective: 1500;
  -webkit-perspective: 1500;
  -moz-perspective: 1500;
  margin-top: 154rpx;
  position: relative;
}
.playcard {
  position: absolute;
  top: 0;
  left: 0;
  transition: all 1s;
  backface-visibility: hidden;
}
.playcard-poker {
  width: 560rpx;
  height: 820rpx;
  text-align: center;
  position: relative;
  margin-top: 25rpx;
}
.playcard-poker .poker-cnt {
  position: relative;
  z-index: 88;
  transform: translateY(180rpx);
}
.playcard-poker .poker-cnt view {
  font-size: 30rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: bold;
  color: #FCCC49;
}
.playcard-poker .poker-cnt view.poker-cnt__score {
  font-size: 150rpx;
  font-family: DINCondensedC;
  font-weight: bold;
  color: #FFEAC1;
}
.playcard-poker .poker-cnt image {
  width: 288rpx;
  height: 48rpx;
  margin-top: 40rpx;
}
.playcard-poker .pokerImg {
  width: 560rpx;
  height: 820rpx;
  position: absolute;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  z-index: 1;
}
.playcard-poker .poker__score {
  position: relative;
  z-index: 88;
  transform: translateY(-999rpx);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.playcard-poker .poker__score.active {
  transform: translateY(82rpx);
}
.playcard-poker .poker__score--detail {
  width: 310rpx;
  height: 50rpx;
  margin: 0 auto;
  position: relative;
  z-index: 88;
  transform: translateY(168rpx);
  background: linear-gradient(270deg, rgba(226, 69, 64, 0) 0%, #F4885B 46%, rgba(222, 68, 62, 0) 100%);
}
.playcard-poker .poker__score--detail text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFE9DA;
}
.playcard-poker .poker__score--detail image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 8rpx;
  vertical-align: middle;
}
.playcard-poker .poker__score__total {
  font-size: 40rpx;
  line-height: 56rpx;
  font-weight: bold;
  color: #FFFFFF;
}
.playcard-poker .poker__score__each {
  width: 220rpx;
  margin: 0 auto 12rpx;
  text-align: right;
  display: flex;
  justify-content: space-between;
}
.playcard-poker .poker__score__each text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFE0A2;
}
.playcard-poker .shareBtn {
