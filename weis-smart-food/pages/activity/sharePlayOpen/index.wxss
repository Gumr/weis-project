  padding-right: 20rpx;
  height: 30rpx;
  line-height: 30rpx;
}
.topname view {
  max-width: 100rpx;
  white-space: nowrap;
  /*规定段落中的文本不进行换行*/
  overflow: hidden;
  /*内容会被修剪，并且其余内容是不可见的。*/
  text-overflow: ellipsis;
  /*显示省略号来代表被修剪的文本*/
}
