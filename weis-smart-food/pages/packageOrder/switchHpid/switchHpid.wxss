.border {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.wrap {
  display: flex;
  align-items: center;
  justify-content: center;
}
.wrap .type {
  color: #666666;
  font-size: 32rpx;
  line-height: 32rpx;
}
.wrap .type:nth-of-type(1),
.wrap .type:nth-of-type(2) {
  margin-right: 40rpx;
}
.wrap .type.active {
  color: #333333;
  font-weight: bold;
  position: relative;
}
.wrap .type.active::after {
  display: block;
  content: '';
  width: 64rpx;
  height: 8rpx;
  background: #FA6400;
  border-radius: 4rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -15rpx;
}
.cutAddress {
  width: 100%;
  padding-top: 112rpx;
}
.cutAddress .head {
  width: 100%;
  height: 112rpx;
  background: #fff;
  padding: 32rpx 0rpx 0rpx 30rpx;
  display: flex;
  align-items: center;
  position: fixed;
  left: 0;
  z-index: 100;
}
.cutAddress .head .address {
  display: flex;
  align-items: center;
  margin-right: 50rpx;
  position: relative;
}
.cutAddress .head .address .addess-icon {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.cutAddress .head .address .name {
  color: #333333;
  font-size: 32rpx;
  font-weight: bold;
  margin-right: 10rpx;
}
.cutAddress .head .address .arrow {
  width: 30rpx;
  height: 30rpx;
  transition: all 200ms ease;
}
.cutAddress .head .address .arrow.active {
  transform: rotate(90deg);
}
.cutAddress .head .address .shadow {
  position: absolute;
  left: 16rpx;
  top: 48rpx;
  width: 136rpx;
  height: 0;
  background: rgba(51, 51, 51, 0.9);
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.cutAddress .head .address .shadow.active {
  animation: anim 200ms ease forwards;
}
@keyframes anim {
  0% {
    height: 0;
  }
  100% {
    height: 200rpx;
  }
}
.cutAddress .head .address .shadow::before {
  position: absolute;
  content: '';
  border-top: 10rpx transparent dashed;
  border-left: 10rpx transparent dashed;
  border-right: 10rpx transparent dashed;
  border-bottom: 10rpx rgba(51, 51, 51, 0.9) solid;
  top: -18rpx;
  left: 50%;
  transform: translateX(-50%);
}
.cutAddress .head .address .shadow .list {
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.cutAddress .head .address .shadow .list:last-of-type {
  margin-bottom: 0;
}
.cutAddress .head .ipt {
  width: 486rpx;
  height: 64rpx;
  background: rgba(233, 233, 233, 0.5);
  border-radius: 33rpx;
  box-sizing: border-box;
  padding-left: 24rpx;
  display: flex;
  align-items: center;
}
.cutAddress .head .ipt image {
  width: 28rpx;
  height: 28rpx;
}
.cutAddress .head .ipt input {
  flex: 1;
  height: 100%;
  padding-left: 12rpx;
}
.cutAddress .head .ipt .input-placeholder {
  color: #a9a9a9;
  font-size: 28rpx;
}
.cutAddress .cur {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  position: relative;
}
.cutAddress .cur::after {
  display: block;
  content: '';
  width: 750rpx;
  height: 2rpx;
  background: #f3f4f5;
  position: absolute;
  left: 0;
  bottom: 0;
  transform: scaleY(0.5);
}
.cutAddress .cur .address {
  max-width: 520rpx;
  color: #333333;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 32rpx;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
  white-space: nowrap;
  -webkit-line-clamp: 1;
}
.cutAddress .cur .right-icon {
  height: 100%;
  display: flex;
  align-items: center;
}
.cutAddress .cur .right-icon image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 12rpx;
}
.cutAddress .cur .right-icon .reLocation {
  color: #fa6400;
  font-size: 28rpx;
}
.cutAddress .myAddress {
  width: 100%;
  padding: 0 30rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cutAddress .myAddress .left-icon {
  display: flex;
  align-items: center;
  padding: 30rpx 0 10rpx;
}
.cutAddress .myAddress .left-icon image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.cutAddress .myAddress .left-icon .name {
  color: #999999;
  font-size: 28rpx;
}
.cutAddress .myAddress .add {
  color: #fa6400;
  font-size: 28rpx;
  padding: 30rpx 0 10rpx;
}
.cutAddress .wrap-address .list {
  width: 100%;
  padding: 30rpx 0 30rpx 30rpx;
  background: #fff;
  display: flex;
  align-items: center;
  position: relative;
}
.cutAddress .wrap-address .list::after {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 690rpx;
  bottom: 0;
}
.cutAddress .wrap-address .list .col-l {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.cutAddress .wrap-address .list .col-l .name {
  font-size: 28rpx;
  color: #333333;
  font-weight: bold;
  margin-bottom: 14rpx;
}
.cutAddress .wrap-address .list .col-l .bot {
  display: flex;
  align-items: center;
}
.cutAddress .wrap-address .list .col-l .bot .tag-box {
  display: flex;
  align-items: center;
}
.cutAddress .wrap-address .list .col-l .bot .tag-box .tag {
  width: 60rpx;
  height: 24rpx;
  background: rgba(254, 94, 15, 0.1);
  box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(164, 180, 183, 0.2);
  color: #fe5e0f;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 12rpx;
}
.cutAddress .wrap-address .list .col-l .bot .tag-box .tag.default {
  background: rgba(254, 94, 15, 0.2);
  color: #fe5e0f;
  margin-right: 8rpx;
}
.cutAddress .wrap-address .list .col-l .bot .tag-box .tag.home {
  background: rgba(249, 67, 67, 0.2);
  color: #f94343;
}
.cutAddress .wrap-address .list .col-l .bot .tag-box .tag.company {
  background: rgba(65, 196, 140, 0.2);
  color: #41c48c;
}
.cutAddress .wrap-address .list .col-l .bot .info {
  color: #808080;
  font-size: 24rpx;
}
.cutAddress .wrap-address .list .col-l .bot .info text {
  margin-right: 10rpx;
}
.cutAddress .wrap-address .list .col-r {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.cutAddress .empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 142rpx;
}
.cutAddress .empty image {
  width: 320rpx;
  height: 240rpx;
}
.cutAddress .empty view {
  color: #A9A9A9;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: center;
}
.cutAddress .search .list {
  padding: 30rpx 26rpx 30rpx 30rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  position: relative;
}
.cutAddress .search .list::after {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 690rpx;
  bottom: 0;
}
.cutAddress .search .list .left {
  max-width: 1460rpx;
}
.cutAddress .search .list .left .title {
  color: #333333;
  font-size: 28rpx;
  line-height: 28rpx;
  font-weight: bold;
  margin-bottom: 14rpx;
}
.cutAddress .search .list .left .address {
  color: #808080;
  font-size: 24rpx;
  line-height: 32rpx;
  width: 530rpx;
}
.cutAddress .search .list .distance {
  color: #808080;
  font-size: 24rpx;
}
.cutAddress .hpid-list .a-box {
  width: 690rpx;
  height: 242rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 24rpx -6rpx rgba(186, 186, 186, 0.3);
  border-radius: 10rpx;
  margin: 0 auto 20rpx;
  display: flex;
}
.cutAddress .hpid-list .a-box:first-of-type {
  margin-top: 10rpx;
}
.cutAddress .hpid-list .a-box .box-l {
  width: 542rpx;
  box-sizing: border-box;
  padding: 0 22rpx 0 30rpx;
  position: relative;
}
.cutAddress .hpid-list .a-box .box-l::after {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  width: 2rpx;
  height: 182rpx;
  transform: scaleX(0.5);
  left: initial;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.cutAddress .hpid-list .a-box .box-l .title {
  display: flex;
  align-items: center;
  margin: 34rpx 0 7rpx 0;
}
.cutAddress .hpid-list .a-box .box-l .title .name {
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
  margin-right: 20rpx;
}
.cutAddress .hpid-list .a-box .box-l .title .can {
  padding: 0 15rpx;
  height: 32rpx;
  border: 2rpx solid #808080;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  color: #808080;
  font-size: 20rpx;
  margin-right: 12rpx;
  border-radius: 16rpx;
}
.cutAddress .hpid-list .a-box .box-l .title .can.active {
  border: 1rpx solid #a9a9a9;
  color: #333333;
}
.cutAddress .hpid-list .a-box .box-l .box-c {
  display: flex;
  align-items: center;
  margin-bottom: 8rpx;
}
.cutAddress .hpid-list .a-box .box-l .box-c image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 4rpx;
}
.cutAddress .hpid-list .a-box .box-l .box-c .distance {
  color: #666666;
  font-size: 20rpx;
}
.cutAddress .hpid-list .a-box .box-l .info {
  width: 100%;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 28rpx;
  word-wrap: break-word;
  margin-bottom: 8rpx;
}
.cutAddress .hpid-list .a-box .box-l .time {
  color: #666666;
  font-size: 20rpx;
  line-height: 28rpx;
}
.cutAddress .hpid-list .a-box .box-l .remain {
  width: fit-content;
  padding: 0 13rpx;
  height: 36rpx;
  background: #FFEEE3;
  border-radius: 18rpx;
  line-height: 36rpx;
  color: #FA6400;
  font-size: 20rpx;
  margin-top: 16rpx;
}
.cutAddress .hpid-list .a-box .box-r {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.cutAddress .hpid-list .a-box .box-r .xd {
  width: 100%;
  height: 171rpx;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
  padding: 79rpx 0 0 0;
  color: #fa6400;
  font-size: 24rpx;
}
.cutAddress .hpid-list .a-box .box-r .icon-b {
  display: flex;
  align-items: center;
}
.cutAddress .hpid-list .a-box .box-r .icon-b image {
  width: 44rpx;
  height: 44rpx;
}
.cutAddress .hpid-list .a-box .box-r .icon-b image:nth-of-type(1) {
  margin-right: 16rpx;
}
.cutAddress .hpid-list .a-box .box-r.active {
  background: linear-gradient(90deg, #fb892d 0%, #fe5e0f 100%);
  box-shadow: 0rpx 2rpx 24rpx 0rpx rgba(102, 102, 102, 0.15);
}
.cutAddress .hpid-list .a-box .box-r.active .xd {
  color: #ffffff;
}
.cutAddress .hpid-list .a-box .box-r.machine-info {
  justify-content: center;
}
.cutAddress .hpid-list .a-box .box-r.machine-info image {
  width: 44rpx;
  height: 44rpx;
}
.cutAddress .hpid-list .a-box .box-r.machine-info image:first-of-type {
  margin-bottom: 48rpx;
}
.cutAddress .hpid-list .a-box.active {
  border: 2rpx solid #fa6400;
}
