.article {
  position: relative;
}
.article__closeimg {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  left: 0;
  top: 0;
  transform: rotate(-90deg);
}
.article__image {
  width: 100%;
  height: 360rpx;
  border-radius: 4rpx;
  display: flex;
  flex-shrink: 0;
}
.article__bottom {
  padding: 0 30rpx;
}
.article__title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  line-height: 32rpx;
  padding: 40rpx 0 30rpx;
}
.article__text {
  font-size: 28rpx;
  color: #666666;
  line-height: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.d-box {
