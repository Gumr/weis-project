.pay {
  width: 100%;
  height: 100%;
  padding-top: 90rpx;
}
.pay.machine-box {
  background: #F3F4F5;
}
.pay-fail {
  padding-top: 100rpx;
}
.pay.pay-succ-box {
  padding-top: 0;
}
.pay .head {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 36rpx;
}
.pay .head image {
  width: 180rpx;
  height: 180rpx;
  margin-bottom: 34rpx;
}
.pay .head .status {
  color: #000000;
  font-size: 40rpx;
  font-weight: 550;
  text-align: center;
}
.pay .pay-succ {
  text-align: center;
  background: linear-gradient(360deg, rgba(253, 157, 0, 0) 0%, #FA6400 100%);
}
.pay .pay-succ-head {
  padding: 50rpx 0;
  text-align: center;
}
.pay .pay-succ-head .icon {
  width: 56rpx;
  height: 56rpx;
  margin-right: 16rpx;
  vertical-align: middle;
}
.pay .pay-succ-head text {
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 56rpx;
  vertical-align: middle;
}
.pay .pay-succ-code {
  width: 650rpx;
  height: 600rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 56rpx 0 44rpx;
  margin-bottom: 20rpx;
}
.pay .pay-succ-code .qrcode {
  width: 346rpx;
  height: 346rpx;
  margin-bottom: 24rpx;
}
.pay .pay-succ-code .code {
  margin-bottom: 12rpx;
  font-size: 68rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 68rpx;
}
.pay .pay-succ-code .tip {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #808080;
  line-height: 26rpx;
}
.pay .pay-succ-invite {
  width: 650rpx;
  height: 180rpx;
  position: relative;
  margin: 0 auto 80rpx;
  padding: 40rpx;
}
.pay .pay-succ-invite .icon {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.pay .pay-succ-invite .title {
  font-size: 38rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #78570F;
  line-height: 53rpx;
  margin-bottom: 6rpx;
  text-align: left;
}
.pay .pay-succ-invite .tip {
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #78570F;
  line-height: 30rpx;
  text-align: left;
  margin-top: 0;
}
.pay .pay-succ-invite .tip text {
  color: #FA6400;
}
.pay .btn-box {
  display: flex;
  justify-content: center;
  align-items: center;
}
.pay .btn-box .btn {
  width: 280rpx;
  height: 88rpx;
  border-radius: 44rpx;
  border: 2rpx solid #fa6400;
  text-align: center;
  line-height: 88rpx;
  color: #fa6400;
  font-size: 32rpx;
}
.pay .btn-box .btn:first-of-type {
  margin-right: 20rpx;
}
.pay .btn-box .btn.fail {
  background: linear-gradient(135deg, #ff9722 0%, #fe5e0f 100%);
  color: #fff;
}
.pay .btn-box.machine-btn .btn:nth-of-type(2) {
  background: linear-gradient(135deg, #FF9722 0%, #FE5E0F 100%);
  border: none;
  color: #ffffff;
}
