page {
  height: 100%;
  background: #FFFFFF;
}
.preference {
  height: 100%;
}
.preference .header {
  width: 750rpx;
  height: 250rpx;
  background: #FFFFFF;
  border-radius: 0rpx 0rpx 30rpx 30rpx;
  padding: 20rpx 0 0 30rpx;
}
.preference .header .title {
  color: #999999;
  font-size: 24rpx;
  margin-bottom: 40rpx;
}
.preference .header .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.preference .header .info {
  display: flex;
  align-items: baseline;
}
.preference .header .info .val {
  display: flex;
  align-items: baseline;
  color: #333333;
  font-size: 70rpx;
  font-family: 'DINCondensedC';
}
.preference .header .info .val .unit {
  font-family: normal;
  color: #666666;
  font-size: 28rpx;
  margin-left: 8rpx;
}
.preference .header .info .adjust {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}
.preference .header .info .adjust image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.preference .header .info .adjust view {
  color: #999999;
  font-size: 26rpx;
}
.preference .content {
  width: 750rpx;
  padding: 30rpx 0 245rpx 30rpx;
  background: #FFFFFF;
}
.preference .content .box {
  margin-bottom: 80rpx;
}
.preference .content .box:last-of-type {
  margin-bottom: 0;
}
.preference .content .box .label {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
}
.preference .content .box .list {
  display: flex;
  flex-wrap: wrap;
}
.preference .content .box .list .tag {
  padding: 0 30rpx;
  height: 52rpx;
  background: #F9F9F9;
  border-radius: 29rpx;
  border: 2rpx solid #E9E9E9;
  margin: 30rpx 30rpx 0 0;
  color: #333333;
  font-size: 26rpx;
  line-height: 48rpx;
}
.preference .content .box .list .tag.active {
  background: #FA6400;
  color: #FFFFFF;
}
.fix {
  width: 100%;
  height: 128rpx;
  background: #FFFFFF;
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0rpx;
  z-index: 10;
}
.fix .btn {
  width: 650rpx;
  height: 88rpx;
  margin: 0 auto;
  background: linear-gradient(90deg, #FB892D 0%, #FE5E0F 100%);
  border-radius: 50rpx;
  text-align: center;
  line-height: 88rpx;
  color: #FFFFFF;
  font-size: 26rpx;
}
.dialog-title {
  text-align: center;
  color: #000000;
  font-size: 32rpx;
  padding: 35rpx 0 55rpx;
  margin: 0 auto;
  position: relative;
  font-weight: bold;
}
.dialog-title .close-icon {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  left: 35rpx;
  top: 35rpx;
}
.ruler-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.ruler-box .title {
  color: #999999;
  font-size: 24rpx;
  margin-bottom: 10rpx;
}
.ruler-box .box {
  width: 168rpx;
  display: flex;
  padding: 0 0 6rpx;
  justify-content: center;
  align-items: baseline;
  color: #333333;
  border-bottom: 2rpx solid #d8d7db;
}
.ruler-box .box input {
  font-size: 48rpx;
  text-align: center;
}
.ruler-box .box .unit {
  font-size: 24rpx;
}
.ruler-box .percent {
  color: #999999;
  font-size: 24rpx;
  margin-top: 2rpx;
}
.ruler-box .ruler {
  margin: 17rpx auto 0;
}
.wrap-nut {
  display: flex;
  justify-content: center;
  margin: 60rpx auto 0;
}
.wrap-nut .ruler-box:not(:last-of-type) {
  margin-right: 87rpx;
}
.wrap-nut .ruler-box .box {
  width: 132rpx;
}
.bot-rule {
  margin: 40rpx auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.bot-rule .reset {
  width: 240rpx;
  height: 88rpx;
  background: #ffffff;
  border-radius: 44rpx;
  border: 2rpx solid #fa6400;
  text-align: center;
  line-height: 84rpx;
  background: #fff;
  color: #fa6400;
  font-size: 37rpx;
  margin-right: 20rpx;
  position: relative;
}
.bot-rule .reset .origin {
  position: absolute;
  top: -16rpx;
  right: 0rpx;
  width: 210rpx;
  height: 61rpx;
  z-index: 10;
}
.bot-rule .reset .origin view {
  width: 100%;
  height: 100%;
  margin-top: -26rpx;
  color: #ffffff;
  font-size: 24rpx;
  text-align: center;
  position: relative;
  z-index: 11;
}
.bot-rule .reset .origin image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.bot-rule .save {
  width: 370rpx;
  height: 88rpx;
  background: linear-gradient(90deg, #fb892d 0%, #fe5e0f 100%);
  border-radius: 50rpx;
  text-align: center;
  line-height: 88rpx;
  color: #ffffff;
  font-size: 36rpx;
}
