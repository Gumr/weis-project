page {
  height: 100%;
  background: #F3F4F5;
}
.flex {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.ab {
  position: absolute;
  left: 0;
  top: 0;
}
.ab-center {
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.healthScore-overview {
  width: 100%;
  position: relative;
  padding: 38rpx 30rpx 0;
  box-sizing: border-box;
}
.healthScore-overview .overview-bg {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.healthScore-overview .overview-bg image {
  position: absolute;
  left: 0;
  top: 0;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 388rpx;
  transform: translate(-50%, -15%);
}
.healthScore-overview .overview-info {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
  align-items: flex-start;
  width: 100%;
}
.healthScore-overview .overview-info-man {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.healthScore-overview .overview-info-man image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 16rpx;
  border-radius: 50%;
}
.healthScore-overview .overview-info-man text {
  font-size: 28rpx;
  font-weight: 400;
  color: #7a3e00;
}
.healthScore-overview .overview-info-meal {
  font-size: 30rpx;
  font-family: Alibaba-PuHuiTi-R, Alibaba-PuHuiTi;
  font-weight: normal;
  color: #7a3e00;
}
.healthScore-overview .overview-score {
  text-align: center;
  margin-top: 20rpx;
}
.healthScore-overview .overview-score-fen {
  text-align: left;
}
.healthScore-overview .overview-score-fen text {
  color: #7a3e00;
}
.healthScore-overview .overview-score-fen text:first-of-type {
  font-size: 140rpx;
  line-height: 140rpx;
  font-family: DINCondensedC;
  font-weight: bold;
}
.healthScore-overview .overview-score-fen text:last-of-type {
  font-size: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
}
.healthScore-overview .overview-score-tip {
  text-align: left;
  font-size: 20rpx;
  font-family: Alibaba-PuHuiTi-R, Alibaba-PuHuiTi;
  font-weight: normal;
  color: #7a3e00;
  margin-top: 8rpx;
}
.box {
  width: 100%;
  box-sizing: border-box;
  padding-bottom: 80rpx;
}
.box .list {
  width: 690rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 20rpx 0rpx rgba(164, 180, 183, 0.2);
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 30rpx;
  margin: 20rpx auto 0;
}
.box .list:first-of-type {
  margin-top: 30rpx;
}
.box .list .category {
  color: #333333;
  font-size: 24rpx;
  font-weight: 550;
  margin-bottom: 40rpx;
}
.box .list .skuList {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-bottom: 60rpx;
}
.box .list .skuList:last-of-type {
  margin-bottom: 0;
}
.box .list .skuList .sku-l {
  display: flex;
}
.box .list .skuList .sku-l .img {
  position: relative;
  margin-right: 36rpx;
}
.box .list .skuList .sku-l .img.active {
  border-radius: 12rpx;
}
.box .list .skuList .sku-l .img image {
  width: 140rpx;
  height: 140rpx;
}
.box .list .skuList .sku-l .img .shadow {
  width: 100%;
  height: 100%;
  position: absolute;
  right: 0;
  top: 0;
  background: #D8D8D880;
  border-radius: 12rpx;
  display: flex;
  align-items: flex-end;
}
.box .list .skuList .sku-l .img .shadow .tag {
  width: 120rpx;
  height: 32rpx;
  background: #00000099;
  border-radius: 0px 0px 12px 12px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #FFFFFF;
  font-size: 20rpx;
}
.box .list .skuList .sku-l .info-box .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: 550;
  margin-bottom: 10rpx;
}
.box .list .skuList .sku-l .info-box .tag-s {
  height: 32rpx;
  display: flex;
  align-items: center;
}
.box .list .skuList .sku-l .info-box .tag-s .small {
  width: fit-content;
  height: 32rpx;
  padding: 0 16rpx;
  background: #eeeeee;
  border-radius: 16rpx;
  color: #000000;
  font-size: 18rpx;
  margin-right: 14rpx;
}
.box .list .skuList .sku-l .info-box .price {
  color: #FF5959;
  font-weight: 550;
  display: flex;
  align-items: flex-end;
  margin-top: 20rpx;
}
.box .list .skuList .sku-l .info-box .price.active {
  color: #A9A9A9;
}
.box .list .skuList .sku-l .info-box .price .symbol {
  font-size: 24rpx;
  margin-bottom: 5rpx;
}
.box .list .skuList .sku-l .info-box .price .money {
  font-size: 32rpx;
}
.box .list .skuList .num {
  color: #A9A9A9;
  font-size: 24rpx;
}
