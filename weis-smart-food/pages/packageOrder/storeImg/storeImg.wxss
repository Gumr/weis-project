page {
  width: 100%;
  background: #F3F4F5;
}
.store {
  background: #F3F4F5;
  width: 100%;
  padding: 20rpx 0 0 40rpx;
  display: flex;
  flex-direction: column;
}
.store .img {
  width: 210rpx;
  height: 210rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.store .img:nth-of-type(3n) {
  margin-right: 0;
}
.store .img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
