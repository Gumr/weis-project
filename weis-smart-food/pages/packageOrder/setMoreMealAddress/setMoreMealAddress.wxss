.border {
  display: block;
  content: "";
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
page {
  background: #f3f4f5;
}
.meal {
  width: 100%;
  background: #f3f4f5;
  padding-bottom: 160rpx;
}
.meal .main-title {
  color: #000000;
  font-size: 32rpx;
  line-height: 45rpx;
  margin: 20rpx 0 20rpx 30rpx;
  font-weight: bold;
}
.meal .head {
  width: 690rpx;
  border-radius: 10rpx 10rpx 0 0;
  background: #fff;
  padding-bottom: 30rpx;
  margin: 0 auto;
}
.meal .flex {
  display: flex;
}
.meal .switch {
  background: #e9e9e9;
}
.meal .switch .disp-card {
  width: 345rpx;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  color: rgba(0, 0, 0, 0.4);
  font-size: 32rpx;
  background: #e9e9e9;
  position: relative;
}
.meal .switch .disp-card text {
  position: relative;
  z-index: 1;
}
.meal .switch .disp-card__icon {
  position: absolute;
  top: 0;
  right: 0;
  width: 50rpx;
  height: 30rpx;
}
.meal .switch .disp-card:first-child {
  border-radius: 10rpx 0 10rpx 0;
}
.meal .switch .disp-card:nth-child(2) {
  border-radius: 0 10rpx 0 10rpx;
}
.meal .switch .disp-card--active {
  color: rgba(0, 0, 0, 0.85);
  background: #fff;
  border-radius: 10rpx 10rpx 0 0 !important;
  font-weight: bold;
}
.meal .switch .disp-card--active::after {
  display: block;
  content: "";
  width: 144rpx;
  height: 20rpx;
  background: linear-gradient(90deg, #fd9d00 0%, #fa6400 100%);
  position: absolute;
  left: 0;
  right: 0;
  bottom: 19rpx;
  margin: auto;
}
.meal .switch .disp-card .select-car {
  width: 366rpx;
  height: 80rpx;
  position: absolute;
  top: -10rpx;
  z-index: 0;
}
.meal .switch .disp-card .select-car-0 {
  left: 0;
}
.meal .switch .disp-card .select-car-1 {
  right: 0;
}
.meal .ps-type {
  display: flex;
  align-items: center;
  margin-top: 35rpx;
}
.meal .ps-type .title {
  color: #333333;
  font-size: 28rpx;
  margin: 0 48rpx 0 30rpx;
}
.meal .ps-type .type-card {
  align-items: center;
}
.meal .ps-type .type-card .select-card--item {
  width: 220rpx;
  height: 80rpx;
  background: #f2f3f4;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
  color: #333333;
  font-size: 28rpx;
}
.meal .ps-type .type-card .select-card--item:nth-child(1) {
  margin-right: 20rpx;
}
.meal .ps-type .type-card .select-card--item.active {
  background: linear-gradient(90deg, #fb892d 0%, #fe5e0f 100%);
  color: #fff;
}
.meal .ps-type .type-card .select-card--item.item--disabled {
  color: #a9a9a9;
  background: #f4f4f4;
}
.meal .c-box .category {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}
.meal .c-box .category:last-of-type {
  margin-bottom: 200rpx;
}
.meal .c-box .category .box {
  width: 690rpx;
  height: 124rpx;
  background: #FFFFFF;
  border-radius: 10px 10rpx 0 0;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
}
.meal .c-box .category .box.active {
  border-radius: 10rpx;
}
.meal .c-box .category .box image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 20rpx;
}
.meal .c-box .category .box .desc {
  color: rgba(0, 0, 0, 0.85);
  font-size: 32rpx;
  font-weight: bold;
}
.meal .c-box .category .info {
  height: 0rpx;
  overflow: hidden;
  transition: all 0.3s linear;
}
.meal .c-box .category .info.show {
  height: 232rpx;
}
.meal .c-box .category .info .address {
  width: 690rpx;
  height: 132rpx;
  background: #FFFFFF;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 34rpx 0 30rpx;
}
.meal .c-box .category .info .address:last-of-type {
  height: 100rpx;
  border-radius: 0rpx 0rpx 10rpx 10rpx;
}
.meal .c-box .category .info .address .col {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.meal .c-box .category .info .address .col .location {
  color: #000000;
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: bold;
}
.meal .c-box .category .info .address .col .name {
  color: #666666;
  font-size: 24rpx;
  line-height: 32rpx;
}
.meal .c-box .category .info .address .tittle {
  color: #000000;
  font-size: 28rpx;
}
.meal .c-box .category .info .address .time {
  display: flex;
  align-items: center;
}
.meal .c-box .category .info .address .time .val {
  color: #FA6400;
  font-size: 28rpx;
  margin-right: 8rpx;
  font-weight: bold;
}
.meal .c-box .category .info .address .arrow {
  width: 32rpx;
  height: 32rpx;
}
.getTime {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 10000;
}
.getTime .empty {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  bottom: 0;
  z-index: -1;
}
.getTime .wrap {
  width: 100%;
  background: #ffffff;
  position: absolute;
  left: 0;
  bottom: -100%;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}
.getTime .wrap.timePicker {
  padding-bottom: 84rpx;
}
.getTime .wrap.active {
  animation: timebox 0.3s ease forwards;
}
@keyframes timebox {
  from {
    bottom: -100%;
  }
  to {
    bottom: 0;
  }
}
.getTime .wrap .head {
  height: 96rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 28rpx;
  position: relative;
}
.getTime .wrap .head.select-head {
  justify-content: center;
}
.getTime .wrap .head .col-l {
  color: #333333;
  font-weight: 400;
}
.getTime .wrap .head .name {
  font-size: 32rpx;
  color: #333333;
  font-weight: 550;
}
.getTime .wrap .head .sure {
  color: #fe5e0f;
}
.getTime .wrap .sure-btn {
  width: 690rpx;
  height: 88rpx;
  background: #A9A9A9;
  box-shadow: 0rpx 6rpx 14rpx 0rpx rgba(169, 169, 169, 0.5);
  border-radius: 44rpx;
  text-align: center;
  line-height: 88rpx;
  color: #ffffff;
  font-size: 30rpx;
  font-weight: 550;
  margin: 0 auto;
}
.getTime .wrap .sure-btn.active {
  background: linear-gradient(180deg, #ff9722 0%, #fe5e0f 100%);
  box-shadow: 0rpx 6rpx 14rpx 0rpx rgba(254, 94, 15, 0.5);
}
.getTime .wrap .box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.getTime .wrap .box .time {
  height: 96rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.getTime .wrap .box .time::after {
  display: block;
  content: "";
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 0;
}
.getTime .wrap .box .time:last-of-type::after {
  display: none;
}
.getTime .wrap .box .time .txt {
  color: #333333;
  font-size: 28rpx;
}
.getTime .wrap .box .time .txt.active {
  color: #fe5e0f;
}
.getTime .wrap .box .time image {
  width: 20rpx;
  height: 20rpx;
}
.pickerCol {
  color: #FA6400 !important;
  font-size: 48rpx !important;
  font-weight: 550;
}
.complete {
  width: 650rpx;
  height: 88rpx;
  background: #A9A9A9;
  border-radius: 50rpx;
  text-align: center;
  line-height: 88rpx;
  color: #FFFFFF;
  font-size: 36rpx;
  position: fixed;
  left: 50%;
  bottom: 70rpx;
  transform: translateX(-50%);
}
.complete.active {
  background: linear-gradient(90deg, #FB892D 0%, #FE5E0F 100%);
}
