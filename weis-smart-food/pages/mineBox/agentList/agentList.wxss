.border {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.agent {
  width: 750rpx;
  padding: 124rpx 30rpx 0;
  position: relative;
  box-sizing: border-box;
}
.agent .search-wrap {
  display: flex;
  justify-content: center;
  padding: 21rpx 0 40rpx;
  background: #ffffff;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  margin: auto;
}
.agent .search-wrap::before {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 0;
}
.agent .search-wrap .search {
  width: 690rpx;
  height: 64rpx;
  background: rgba(233, 233, 233, 0.5);
  border-radius: 33rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0 0 0 24rpx;
}
.agent .search-wrap .search image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 12rpx;
}
.agent .search-wrap .search input {
  flex: 1;
  height: 100%;
  color: #333333;
  font-size: 28rpx;
}
.agent .search-wrap .search input.ipt {
  color: #A9A9A9;
  font-size: 28rpx;
}
.agent .list .list-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 60rpx;
}
.agent .list .list-box .list-left {
  display: flex;
  align-items: center;
}
.agent .list .list-box .list-left image {
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.agent .list .list-box .list-left .c-box {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.agent .list .list-box .list-left .c-box .t-cont {
  color: #333333;
  font-size: 28rpx;
  font-weight: 550;
  margin-bottom: 14rpx;
}
.agent .list .list-box .list-left .c-box .b-cont {
  color: #808080;
  font-size: 24rpx;
}
.agent .list .list-box .add {
  box-sizing: border-box;
  width: 96rpx;
  height: 40rpx;
  border-radius: 20rpx;
  border: 2rpx solid #808080;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #808080;
  font-size: 24rpx;
}
.agent .list .list-box .add.active {
  color: #808080;
  border-color: #808080;
}
.agent .list .no {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: -1;
}
.agent .list .no image {
  width: 268rpx;
  height: 268rpx;
}
.load {
  margin: 0 auto;
  text-align: center;
}
