page {
  background: #f3f4f5;
  height: 100%;
}
.border {
  display: block;
  content: "";
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.wrap {
  display: flex;
  align-items: center;
  justify-content: center;
}
.wrap .type {
  color: #666666;
  font-size: 32rpx;
  line-height: 32rpx;
}
.wrap .type:nth-of-type(1),
.wrap .type:nth-of-type(2) {
  margin-right: 40rpx;
}
.wrap .type.active {
  color: #333333;
  font-weight: bold;
  position: relative;
}
.wrap .type.active::after {
  display: block;
  content: '';
  width: 64rpx;
  height: 8rpx;
  background: #FA6400;
  border-radius: 4rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -15rpx;
}
.coupon {
  width: 100%;
  height: 100%;
  padding: 30rpx 35rpx 0;
  position: relative;
  background: #f3f4f5;
}
.coupon::before {
  display: block;
  content: "";
  width: 100%;
  height: 2rpx;
  background: #e9e9e9;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 0;
}
.coupon .header-icon {
  width: 680rpx;
  height: 78rpx;
  margin: 0 auto 40rpx;
}
.coupon .tip {
  font-size: 28rpx;
  color: #000000;
  margin-bottom: 52rpx;
}
.coupon .coupon-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.coupon .coupon-box .list {
  position: relative;
  width: 680rpx;
  height: 192rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  color: #ffffff;
  margin-bottom: 32rpx;
}
.coupon .coupon-box .list .bg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.coupon .coupon-box .list .tag {
  width: 144rpx;
  height: 58rpx;
  position: absolute;
  left: 0;
  top: -22rpx;
}
.coupon .coupon-box .list .c-t {
  width: 100%;
  height: 100%;
  display: flex;
  box-sizing: border-box;
  position: absolute;
}
.coupon .coupon-box .list .c-t .c-left {
  width: 192rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: #fff;
}
.coupon .coupon-box .list .c-t .c-left .amount {
  font-size: 36rpx;
  font-family: "DINCondensedC";
  font-weight: bold;
  line-height: 24rpx;
}
.coupon .coupon-box .list .c-t .c-left .amount text {
  display: inline-block;
  font-size: 120rpx;
  font-family: "DINCondensedC";
  font-weight: bold;
  line-height: 90rpx;
  margin-left: -12rpx;
}
.coupon .coupon-box .list .c-t .c-left .limit {
  font-size: 24rpx;
  font-weight: bold;
  line-height: 33rpx;
  margin-top: 10rpx;
}
.coupon .coupon-box .list .c-t .c-right {
  padding: 30rpx 0 20rpx 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.coupon .coupon-box .list .c-t .c-right .name {
  max-width: 256rpx;
  color: #000000;
  font-size: 32rpx;
  line-height: 36rpx;
  overflow: hidden;
  word-break: break-all;
  text-overflow: ellipsis;
  white-space: nowrap;
  -webkit-line-clamp: 2;
  font-weight: bold;
}
.coupon .coupon-box .list .c-t .c-right .bot-box .date {
  font-size: 22rpx;
  color: #A9A9A9;
  margin-bottom: 10rpx;
}
.coupon .coupon-box .list .c-t .c-right .bot-box .userule {
  font-size: 22rpx;
  color: #A9A9A9;
  display: flex;
  align-items: center;
}
.coupon .coupon-box .list .c-t .c-right .bot-box .userule image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 6rpx;
}
.coupon .coupon-box .list .c-t .btn {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  width: 122rpx;
  height: 44rpx;
  border-radius: 22rpx;
  text-align: center;
  border: 2rpx solid #FF9B59;
  line-height: 40rpx;
  color: #FA6400;
  font-size: 24rpx;
  font-weight: bold;
}
.coupon .coupon-box .disable .c-t .c-left .amount {
  color: #fff;
}
.coupon .coupon-box .disable .c-t .c-right .name {
  color: #666666;
}
.coupon .coupon-box .disable .c-t .c-right .rule {
  color: #B1B1B1;
}
.coupon .coupon-box .disable .c-t .c-right .date {
  color: #A9A9A9;
}
.coupon .coupon-box .disable .c-t .c-right .userule {
  color: #A9A9A9;
}
.coupon .coupon-box .disable .c-t .btn {
  background: #D8D8D8;
}
.coupon .no {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  top: 20%;
}
.coupon .no image {
  width: 268rpx;
  height: 268rpx;
}
.coupon .no .desc {
  text-align: center;
  color: #a9a9a9;
  font-size: 22rpx;
}
.coupon .invalid {
  font-size: 28rpx;
  color: #000000;
  line-height: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 80rpx;
}
.coupon .invalid image {
  width: 38rpx;
  height: 38rpx;
  margin-left: 2rpx;
}
