  align-items: center;
  width: 650rpx;
  margin: 0 auto;
  margin-top: 60rpx;
}
.info .sugar .wrap .list {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
  box-sizing: border-box;
  position: relative;
  background: #f2f3f4;
  border-radius: 10rpx;
  margin-right: 20rpx;
  height: 180rpx;
  text-align: center;
}
.info .sugar .wrap .list:last-of-type {
