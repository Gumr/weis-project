.info .overBtn {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}
.info .overBtn .next {
  width: 650rpx;
  height: 88rpx;
  background: #D8D8D8;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  color: #ffffff;
  margin: auto;
  border-radius: 50rpx;
  margin-bottom: 65rpx;
}
.info .overBtn .next.active {
  color: #FFFFFF;
  background: linear-gradient(90deg, #FB892D 0%, #FE5E0F 100%);
}
.info .overBtn .xy {
  width: 568rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: auto;
  margin-bottom: 30rpx;
}
.info .overBtn .xy image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 8rpx;
}
.info .overBtn .xy .xy_cont {
  font-size: 24rpx;
  color: #333333;
}
.info .overBtn .xy .xy_cont text {
  color: #FA6400;
}
.conneTip {
  padding: 0 40rpx 64rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #999999;
  line-height: 40rpx;
}
