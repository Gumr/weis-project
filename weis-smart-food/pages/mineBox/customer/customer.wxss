.mycode {
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  background: #fafafa;
  padding-left: 30rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(0, 0, 0, 0.85);
}
.detail-wrap {
  width: 100%;
}
.detail-wrap .detail-item {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 24rpx 0 24rpx 30rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}
.detail-wrap .detail-item .item-img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.detail-wrap .detail-item .item-cont {
  flex-grow: 1;
}
.detail-wrap .detail-item .item-cont view:nth-child(1) {
  margin-bottom: 16rpx;
  font-size: 30rpx;
  color: #000000;
}
.detail-wrap .detail-item .item-cont view:nth-child(2) {
  font-size: 24rpx;
  color: #666666;
}
.null {
  margin-top: 160rpx;
  text-align: center;
}
.null image {
  width: 268rpx;
  height: 268rpx;
}
.null view {
  font-size: 32rpx;
  color: #999999;
}
