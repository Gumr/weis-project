.header {
  padding: 60rpx 0;
}
.content {
  overflow: hidden;
  background: #ffffff;
  border-radius: 40rpx 40rpx 0rpx 0rpx;
}
.content .tab {
  display: flex;
  position: relative;
}
.content .tab .list {
  padding: 60rpx 0 22rpx;
  flex: 1;
  text-align: center;
  color: rgba(51, 51, 51, 0.5);
  font-size: 28rpx;
}
.content .tab .list.active {
  color: #fe5e0f;
  position: relative;
}
.content .tab .list.active::after {
  display: block;
  content: '';
  width: 24rpx;
  height: 6rpx;
  background: #FE5E0F;
  border-radius: 4rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 10rpx;
}
.content .tab::after {
  display: block;
  content: '';
  width: 100%;
  height: 2rpx;
  background: rgba(51, 51, 51, 0.1);
  transform: translateY(0.5);
  position: absolute;
  left: 0;
  bottom: 0;
}
.label-1 {
  margin: 40rpx 0 44rpx;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 0.7);
}
.label-2 {
  font-size: 28rpx;
  color: #fff;
  margin: 30rpx 0 73rpx;
}
.label-3 {
  font-size: 28rpx;
  color: #6d7278;
  position: relative;
}
.label-3 .absolute {
  width: 196rpx;
  height: 28rpx;
  background: linear-gradient(135deg, #FB892D 0%, #FE5E0F 100%);
  border-radius: 0rpx 14rpx 14rpx 14rpx;
  text-align: center;
  line-height: 28rpx;
  position: absolute;
  left: 122rpx;
  top: 50%;
  transform: translateY(-50%);
  color: #ffffff;
  font-size: 20rpx;
}
.label-4 {
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
}
.label-rule {
  line-height: 1;
