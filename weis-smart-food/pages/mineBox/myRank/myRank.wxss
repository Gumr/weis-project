.btn {
  text-align: center;
  line-height: 68rpx;
  width: 260rpx;
  height: 68rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  border-radius: 34rpx;
}
.btn__yellow {
  position: relative;
  color: #000000;
  background: #ffe600;
}
.btn__border {
  color: #ffe600;
  border: 2rpx solid #ffe600;
}
.btn-tag {
  width: 161rpx;
  height: 42rpx;
  position: absolute;
  right: 0;
  top: -13rpx;
}
.weight-tag {
  margin-left: 10rpx;
  font-size: 20rpx;
  color: #fff;
  padding: 3rpx 12rpx;
  background: linear-gradient(135deg, #fb892d 0%, #fe5e0f 100%);
  border-radius: 0rpx 14rpx 14rpx 14rpx;
}
.content-item {
  padding-bottom: 62rpx;
}
.content-item:nth-child(2) {
  border-bottom: 2rpx solid rgba(51, 51, 51, 0.1);
}
.l-box {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 73rpx;
}
.l-box .btns {
  width: 260rpx;
  height: 68rpx;
  background: #FFE600;
  border-radius: 34rpx;
  position: relative;
  margin-bottom: 0;
}
.l-box .btns:first-of-type {
  margin-right: 70rpx;
}
.l-box .btns .goOrder {
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 68rpx;
  color: #000000;
  font-size: 32rpx;
}
.l-box .btns .btn-tag {
  width: 161rpx;
  height: 42rpx;
