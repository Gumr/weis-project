page {
  background-color: #F6F6F6;
}
.AddressList {
  margin-top: 40rpx;
}
.AddressList .selectAdd {
  width: 690rpx;
  height: 187rpx;
  background: #ffffff;
  border-radius: 10rpx;
  padding-top: 44rpx;
  display: flex;
  margin-bottom: 20rpx;
}
.AddressList .flexname {
  display: flex;
}
.AddressList .addname {
  width: 541rpx;
}
.AddressList .addname image {
  width: 80rpx;
  height: 80rpx;
  margin-left: 30rpx;
}
.AddressList .addname .noright {
  border-right: unset!important;
}
.AddressList .addname .titfont {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  color: #000000;
  padding-left: 30rpx;
  border-right: 1rpx solid #EAEAEA;
}
.AddressList .addname .titfont view {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  margin-top: 19rpx;
  line-height: 1.4;
  padding-right: 10rpx;
}
.AddressList .adddistance {
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
  width: 174rpx;
  padding-top: 24rpx;
}
.AddressList .adddistance text {
  line-height: 1.4;
}
.selectMeals {
  width: 750rpx;
  background: #ffffff;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.selectMeals .meals {
  width: 375rpx;
  text-align: center;
  margin-top: 80rpx;
  border-right: 1rpx solid #EEEEEEFF;
  height: 270rpx;
}
.selectMeals .meals image {
  width: 170rpx;
  height: 170rpx;
}
.selectMeals .meals .Mealtit {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  color: rgba(0, 0, 0, 0.85);
  margin-top: 18rpx;
  margin-bottom: 12rpx;
}
.selectMeals .meals .Mealtips {
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
}
.selectMeals button {
  width: 420rpx;
  height: 88rpx;
  background: #e8e8e8;
  border-radius: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #ffffff;
  margin-top: 159rpx;
  margin-bottom: 20rpx;
}
.search {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  height: 56rpx;
  background: #E5E5E6FF;
  border-radius: 28rpx;
  padding-left: 24rpx;
  padding-right: 8rpx;
  margin-top: 32rpx;
}
.search .sericon {
  width: 48rpx;
  height: 48rpx;
}
.changeUser {
  margin: 40rpx;
}
.changeUser .title {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #000000;
  margin-bottom: 60rpx;
  text-align: center;
}
.changeUser .content .user {
  width: 670rpx;
  height: 180rpx;
  background: #F9F9F9;
  border-radius: 24rpx;
  padding: 30rpx;
  display: flex;
  margin-bottom: 40rpx;
}
.changeUser .content .user image {
  width: 120rpx;
  height: 120rpx;
  border: 2rpx solid #FFFFFF;
  border-radius: 50%;
  vertical-align: middle;
}
.changeUser .content .user .info {
  padding-top: 20rpx;
  padding-left: 24rpx;
}
.changeUser .content .user .info text {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #000000;
}
.changeUser .content .user .info view {
  margin-top: 16rpx;
  border-radius: 18rpx;
  background: #F9E1D5;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FA6400;
  text-align: center;
  width: 134rpx;
}
.changeUser .content .user .info .changetips {
  background: #EAEAEA !important;
  color: #333333 !important;
}
.changeUser .btnlist {
  display: flex;
  margin-top: 70rpx;
}
.changeUser .btnlist button {
  width: 315rpx;
  height: 88rpx;
  border-radius: 44rpx;
}
.changeUser .btnlist .cancel {
  border: 1rpx solid #333333;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
}
.changeUser .btnlist .confrim {
  background: linear-gradient(90deg, #FB892D 0%, #FE5E0F 100%);
  border-radius: 44rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}
